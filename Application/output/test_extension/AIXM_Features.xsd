                </annotation>
            </element>
            <element maxOccurs="1" minOccurs="0"
                name="surfaceProperties" nillable="true" type="aixm:SurfaceCharacteristicsPropertyType">
                <annotation>
                    <documentation>Identifies the surface characteristics of the taxiway element.</documentation>
                </annotation>
