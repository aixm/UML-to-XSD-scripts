        <annotation>
            <documentation>The presence or removal of hazardous conditions due to snow, ice, slush, water, etc. on the runway surface.&#xd;
</documentation>
        </annotation>
    </element>
    <complexType name="RunwayContaminationType">
        <complexContent>
            <extension base="aixm:AbstractSurfaceContaminationType">
                <sequence>
                    <group ref="aixm:SurfaceContaminationPropertyGroup"/>
                    <group ref="aixm:RunwayContaminationPropertyGroup"/>
                    <element maxOccurs="unbounded" minOccurs="0" name="extension">
                        <complexType>
                            <choice>
                                <element ref="aixm:AbstractSurfaceContaminationExtension"/>
                                <element ref="aixm:AbstractRunwayContaminationExtension"/>
                            </choice>
                            <attributeGroup ref="gml:OwnershipAttributeGroup"/>
                        </complexType>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <group name="RunwayContaminationPropertyGroup">
        <sequence>
            <element minOccurs="0" name="clearedLength" nillable="true" type="aixm:ValDistanceType">
                <annotation>
                    <documentation>Length of runway cleared of contamination, if less than the total length of the runway.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="clearedWidth" nillable="true" type="aixm:ValDistanceType">
                <annotation>
                    <documentation>Width of runway cleared of contamination, if less than the total width.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="clearedSide" nillable="true" type="aixm:CodeSideType">
                <annotation>
                    <documentation>Indicates that the cleared width is offset left or right of a runway centre line.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="furtherClearanceLength"
                nillable="true" type="aixm:ValDistanceType">
                <annotation>
                    <documentation>Length of runway that is expected to be cleared of contamination, if less than the total length.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="furtherClearanceWidth"
                nillable="true" type="aixm:ValDistanceType">
                <annotation>
                    <documentation>Width of runway that is expected to be cleared of contamination, if less than the total width.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="obscuredLightsSide"
                nillable="true" type="aixm:CodeSideType">
                <annotation>
                    <documentation>Indicates the side of the runway on which the lights are obscured by contamination.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="clearedLengthBegin"
                nillable="true" type="aixm:ValDistanceType">
                <annotation>
                    <documentation>The distance from the threshold of the runway direction having the lowest designator number to the point where the cleared portion starts, in case of partial clearance.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="taxiwayAvailable"
                nillable="true" type="aixm:CodeYesNoType">
                <annotation>
                    <documentation>An indication that there are cleared taxiways available serving that runway.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="apronAvailable" nillable="true" type="aixm:CodeYesNoType">
                <annotation>
                    <documentation>An indication that there are cleared aprons available serving a runway.</documentation>
                </annotation>
            </element>
        </sequence>
    </group>
    <complexType name="RunwayContaminationPropertyType">
        <complexContent>
            <extension base="aixm:AbstractAIXMPropertyType">
                <sequence>
                    <element ref="aixm:RunwayContamination"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <element abstract="true" name="AbstractRunwayContaminationExtension"
        substitutionGroup="aixm:AbstractExtension" type="aixm:AbstractExtensionType"/>
    <element name="RunwaySectionContamination"
        substitutionGroup="aixm:AbstractSurfaceContamination" type="aixm:RunwaySectionContaminationType">
        <annotation>
            <documentation>The presence or removal of hazardous conditions due to snow, ice, slush, water, etc. on a section of the runway surface.&#xd;
</documentation>
        </annotation>
    </element>
    <complexType name="RunwaySectionContaminationType">
        <complexContent>
            <extension base="aixm:AbstractSurfaceContaminationType">
                <sequence>
                    <group ref="aixm:SurfaceContaminationPropertyGroup"/>
                    <group ref="aixm:RunwaySectionContaminationPropertyGroup"/>
                    <element maxOccurs="unbounded" minOccurs="0" name="extension">
                        <complexType>
                            <choice>
                                <element ref="aixm:AbstractSurfaceContaminationExtension"/>
                                <element ref="aixm:AbstractRunwaySectionContaminationExtension"/>
                            </choice>
                            <attributeGroup ref="gml:OwnershipAttributeGroup"/>
                        </complexType>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <group name="RunwaySectionContaminationPropertyGroup">
        <sequence>
            <element minOccurs="0" name="section" nillable="true" type="aixm:CodeRunwaySectionType">
                <annotation>
                    <documentation>Indicates the part of the runway that is affected. This supports indicating the contamination information on each third of the runway length, starting from the threshold of the runway direction having the lower designation number.</documentation>
                </annotation>
            </element>
        </sequence>
    </group>
    <complexType name="RunwaySectionContaminationPropertyType">
        <complexContent>
            <extension base="aixm:AbstractAIXMPropertyType">
                <sequence>
                    <element ref="aixm:RunwaySectionContamination"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <element abstract="true"
        name="AbstractRunwaySectionContaminationExtension"
        substitutionGroup="aixm:AbstractExtension" type="aixm:AbstractExtensionType"/>
    <element abstract="true" name="AbstractSurfaceContamination" type="aixm:AbstractSurfaceContaminationType">
        <annotation>
            <documentation>Presence or removal of hazardous conditions on aerodrome/heliport movement areas due to snow, ice, slush, water.</documentation>
        </annotation>
    </element>
    <complexType name="AbstractSurfaceContaminationType">
        <complexContent>
            <extension base="aixm:AbstractAIXMObjectType">
                <sequence/>
            </extension>
        </complexContent>
    </complexType>
    <group name="SurfaceContaminationPropertyGroup">
        <sequence>
            <element minOccurs="0" name="observationTime"
                nillable="true" type="aixm:DateTimeType">
                <annotation>
                    <documentation>The date and time of surface contamination measurement completion.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="depth" nillable="true" type="aixm:ValDepthType">
                <annotation>
                    <documentation>The depth of the contamination on the surface.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="frictionCoefficient"
                nillable="true" type="aixm:ValFrictionType">
                <annotation>
                    <documentation>The average ratio of the weight of an object being moved along an airport/heliport surface with contamination and the force that maintains contact between the object and that surface.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="frictionEstimation"
                nillable="true" type="aixm:CodeFrictionEstimateType">
                <annotation>
                    <documentation>A qualitative estimate of the friction on an airport/heliport surface with contamination.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="frictionDevice" nillable="true" type="aixm:CodeFrictionDeviceType">
                <annotation>
                    <documentation>The type of equipment used to determine the friction coefficient of an airport/heliport surface with contamination.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="obscuredLights" nillable="true" type="aixm:CodeYesNoType">
                <annotation>
                    <documentation>Indicates that the surface lights are obscured by surface contamination.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="furtherClearanceTime"
                nillable="true" type="aixm:TimeType">
                <annotation>
                    <documentation>The time when it is expected to complete further clearance of an airport/heliport surface with contamination.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="furtherTotalClearance"
                nillable="true" type="aixm:CodeYesNoType">
                <annotation>
                    <documentation>Indicates that further total clearance of an airport/heliport surface with contamination is expected.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="nextObservationTime"
                nillable="true" type="aixm:DateTimeType">
                <annotation>
                    <documentation>The date and time of surface contamination measurement completion.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="proportion" nillable="true" type="aixm:ValPercentType">
                <annotation>
                    <documentation>The percentage of the contaminated area of the overall extent of the surface.</documentation>
                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0" name="layer"
                nillable="true" type="aixm:SurfaceContaminationLayerPropertyType">
                <annotation>
                    <documentation>A layer of contaminant.</documentation>
                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0"
                name="criticalRidge" nillable="true" type="aixm:RidgePropertyType">
                <annotation>
                    <documentation>Identifies the critical bank associated to the contaminant.</documentation>
                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0"
                name="annotation" nillable="true" type="aixm:NotePropertyType"/>
        </sequence>
    </group>
    <complexType name="SurfaceContaminationPropertyType">
        <complexContent>
            <extension base="aixm:AbstractAIXMPropertyType">
                <sequence>
                    <element ref="aixm:AbstractSurfaceContamination"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <element abstract="true"
        name="AbstractSurfaceContaminationExtension"
        substitutionGroup="aixm:AbstractExtension" type="aixm:AbstractExtensionType"/>
    <element name="SurfaceContaminationLayer" type="aixm:SurfaceContaminationLayerType">
        <annotation>
            <documentation>An operationally significant contaminant of homogeneous type such as snow, ice, slush, water, sand, etc., which is present on a large surface of the airport/heliport area.&#xd;
</documentation>
        </annotation>
    </element>
    <complexType name="SurfaceContaminationLayerType">
        <complexContent>
            <extension base="aixm:AbstractAIXMObjectType">
                <sequence>
                    <group ref="aixm:SurfaceContaminationLayerPropertyGroup"/>
                    <element maxOccurs="unbounded" minOccurs="0" name="extension">
                        <complexType>
                            <choice>
                                <element ref="aixm:AbstractSurfaceContaminationLayerExtension"/>
                            </choice>
                            <attributeGroup ref="gml:OwnershipAttributeGroup"/>
                        </complexType>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <group name="SurfaceContaminationLayerPropertyGroup">
        <sequence>
            <element minOccurs="0" name="layerOrder" nillable="true" type="aixm:NoSequenceType">
                <annotation>
                    <documentation>The order of the layer (starting from the uppermost, towards the movement area surface), in case overlapping contaminants are present.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="type" nillable="true" type="aixm:CodeContaminationType">
                <annotation>
                    <documentation>The type of contaminating substance on the movement area surface.</documentation>
                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0" name="extent"
                nillable="true" type="aixm:ElevatedSurfacePropertyType">
                <annotation>
                    <documentation>The surface covered by the contamination.  For runways this can be a runway third, such as in the current SNOWTAM reporting style..</documentation>
                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0"
                name="annotation" nillable="true" type="aixm:NotePropertyType"/>
        </sequence>
    </group>
    <complexType name="SurfaceContaminationLayerPropertyType">
        <complexContent>
            <extension base="aixm:AbstractAIXMPropertyType">
                <sequence>
                    <element ref="aixm:SurfaceContaminationLayer"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <element abstract="true"
        name="AbstractSurfaceContaminationLayerExtension"
        substitutionGroup="aixm:AbstractExtension" type="aixm:AbstractExtensionType"/>
    <element name="TaxiwayContamination"
        substitutionGroup="aixm:AbstractSurfaceContamination" type="aixm:TaxiwayContaminationType">
        <annotation>
            <documentation>The presence or removal of hazardous conditions due to snow, ice, slush, water, etc. on the taxiway surface.&#xd;
</documentation>
        </annotation>
    </element>
    <complexType name="TaxiwayContaminationType">
        <complexContent>
            <extension base="aixm:AbstractSurfaceContaminationType">
                <sequence>
                    <group ref="aixm:SurfaceContaminationPropertyGroup"/>
                    <group ref="aixm:TaxiwayContaminationPropertyGroup"/>
                    <element maxOccurs="unbounded" minOccurs="0" name="extension">
                        <complexType>
                            <choice>
                                <element ref="aixm:AbstractSurfaceContaminationExtension"/>
                                <element ref="aixm:AbstractTaxiwayContaminationExtension"/>
                            </choice>
                            <attributeGroup ref="gml:OwnershipAttributeGroup"/>
                        </complexType>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <group name="TaxiwayContaminationPropertyGroup">
        <sequence>
            <element minOccurs="0" name="clearedWidth" nillable="true" type="aixm:ValDistanceType">
                <annotation>
                    <documentation>The width of taxiway cleared of contamination.</documentation>
                </annotation>
            </element>
        </sequence>
    </group>
    <complexType name="TaxiwayContaminationPropertyType">
        <complexContent>
            <extension base="aixm:AbstractAIXMPropertyType">
                <sequence>
                    <element ref="aixm:TaxiwayContamination"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <element abstract="true"
        name="AbstractTaxiwayContaminationExtension"
        substitutionGroup="aixm:AbstractExtension" type="aixm:AbstractExtensionType"/>
    <element name="TouchDownLiftOffContamination"
        substitutionGroup="aixm:AbstractSurfaceContamination" type="aixm:TouchDownLiftOffContaminationType">
        <annotation>
            <documentation>The presence or removal of hazardous conditions due to snow, ice, slush, water, etc. on the Touch Down and Lift-Off (TLOF) surface.</documentation>
        </annotation>
    </element>
    <complexType name="TouchDownLiftOffContaminationType">
        <complexContent>
            <extension base="aixm:AbstractSurfaceContaminationType">
                <sequence>
                    <group ref="aixm:SurfaceContaminationPropertyGroup"/>
                    <group ref="aixm:TouchDownLiftOffContaminationPropertyGroup"/>
                    <element maxOccurs="unbounded" minOccurs="0" name="extension">
                        <complexType>
                            <choice>
                                <element ref="aixm:AbstractSurfaceContaminationExtension"/>
                                <element ref="aixm:AbstractTouchDownLiftOffContaminationExtension"/>
                            </choice>
                            <attributeGroup ref="gml:OwnershipAttributeGroup"/>
                        </complexType>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <group name="TouchDownLiftOffContaminationPropertyGroup">
        <sequence/>
    </group>
    <complexType name="TouchDownLiftOffContaminationPropertyType">
        <complexContent>
            <extension base="aixm:AbstractAIXMPropertyType">
                <sequence>
                    <element ref="aixm:TouchDownLiftOffContamination"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <element abstract="true"
        name="AbstractTouchDownLiftOffContaminationExtension"
        substitutionGroup="aixm:AbstractExtension" type="aixm:AbstractExtensionType"/>
    <complexType name="GuidanceLinePropertyType">
        <attributeGroup ref="gml:OwnershipAttributeGroup"/>
        <attributeGroup ref="gml:AssociationAttributeGroup"/>
    </complexType>
    <element name="GuidanceLine"
        substitutionGroup="aixm:AbstractAIXMFeature" type="aixm:GuidanceLineType">
        <annotation>
            <documentation>A line used to guide aircraft on and between airport movement areas.&#xd;
</documentation>
        </annotation>
    </element>
    <complexType name="GuidanceLineType">
        <complexContent>
            <extension base="aixm:AbstractAIXMFeatureType">
                <sequence>
                    <element maxOccurs="unbounded" name="timeSlice" type="aixm:GuidanceLineTimeSlicePropertyType"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="GuidanceLineTimeSlicePropertyType">
        <sequence>
            <element ref="aixm:GuidanceLineTimeSlice"/>
        </sequence>
        <attributeGroup ref="gml:OwnershipAttributeGroup"/>
    </complexType>
    <element name="GuidanceLineTimeSlice"
        substitutionGroup="gml:AbstractTimeSlice" type="aixm:GuidanceLineTimeSliceType"/>
    <complexType name="GuidanceLineTimeSliceType">
        <complexContent>
            <extension base="aixm:AbstractAIXMTimeSliceType">
                <sequence>
                    <group ref="aixm:GuidanceLinePropertyGroup"/>
                    <element maxOccurs="unbounded" minOccurs="0" name="extension">
                        <complexType>
                            <sequence>
                                <element ref="aixm:AbstractGuidanceLineExtension"/>
                            </sequence>
                            <attributeGroup ref="gml:OwnershipAttributeGroup"/>
                        </complexType>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <group name="GuidanceLinePropertyGroup">
        <sequence>
            <element minOccurs="0" name="designator" nillable="true" type="aixm:TextNameType">
                <annotation>
                    <documentation>The identifier of a guidance line on a taxiway.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="type" nillable="true" type="aixm:CodeGuidanceLineType">
                <annotation>
                    <documentation>The type of guidance line on a taxiway.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="maxSpeed" nillable="true" type="aixm:ValSpeedType">
                <annotation>
                    <documentation>The maximum ground speed permitted on a taxiway.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="usageDirection" nillable="true" type="aixm:CodeDirectionType">
                <annotation>
                    <documentation>Indicates the direction in which the guidance line can be used with reference to the start and end points of the associated ElevatedCurve.</documentation>
                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0"
                name="connectedTaxiway" nillable="true" type="aixm:TaxiwayPropertyType">
                <annotation>
                    <documentation>Identifies the taxiway that the guidance line connects to.</documentation>
                </annotation>
            </element>
            <element maxOccurs="1" minOccurs="0" name="extent"
                nillable="true" type="aixm:ElevatedCurvePropertyType">
                <annotation>
                    <documentation>Identifies the extent of the guidance line.</documentation>
                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0"
                name="connectedRunwayCentrelinePoint" nillable="true" type="aixm:RunwayCentrelinePointPropertyType">
                <annotation>
                    <documentation>(OD272/ED99) The value of a RunwayExitLine feature?s attribute idthr shall be identical to the value of the operationally corresponding RunwayThresholds feature?s attribute idthr (Rule 21).</documentation>
                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0"
                name="connectedTouchDownLiftOff" nillable="true" type="aixm:TouchDownLiftOffPropertyType">
                <annotation>
                    <documentation>Identifies the touch down lift off area that the guidance line connects to.</documentation>
                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0"
                name="connectedStand" nillable="true" type="aixm:AircraftStandPropertyType">
                <annotation>
                    <documentation>(DO272/ED99) Each StandGuidanceLine feature?s attribute idstd shall provide object identifier values for all corresponding ParkingStandLocations (Rule 22).</documentation>
                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0"
                name="connectedApron" nillable="true" type="aixm:ApronPropertyType">
                <annotation>
                    <documentation>Identifies the apron that the guidance line connects to.</documentation>
                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0"
                name="annotation" nillable="true" type="aixm:NotePropertyType"/>
        </sequence>
    </group>
    <element abstract="true" name="AbstractGuidanceLineExtension"
        substitutionGroup="aixm:AbstractExtension" type="aixm:AbstractExtensionType"/>
    <complexType name="TaxiHoldingPositionPropertyType">
        <attributeGroup ref="gml:OwnershipAttributeGroup"/>
        <attributeGroup ref="gml:AssociationAttributeGroup"/>
    </complexType>
    <element name="TaxiHoldingPosition"
        substitutionGroup="aixm:AbstractAIXMFeature" type="aixm:TaxiHoldingPositionType">
        <annotation>
            <documentation>A designated position intended for traffic control at which taxiing aircraft and vehicles shall stop and hold until further cleared to proceed, when so instructed by the aerodrome control tower.&#xd;
</documentation>
        </annotation>
    </element>
    <complexType name="TaxiHoldingPositionType">
        <complexContent>
            <extension base="aixm:AbstractAIXMFeatureType">
                <sequence>
                    <element maxOccurs="unbounded" name="timeSlice" type="aixm:TaxiHoldingPositionTimeSlicePropertyType"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="TaxiHoldingPositionTimeSlicePropertyType">
        <sequence>
            <element ref="aixm:TaxiHoldingPositionTimeSlice"/>
        </sequence>
        <attributeGroup ref="gml:OwnershipAttributeGroup"/>
    </complexType>
    <element name="TaxiHoldingPositionTimeSlice"
        substitutionGroup="gml:AbstractTimeSlice" type="aixm:TaxiHoldingPositionTimeSliceType"/>
    <complexType name="TaxiHoldingPositionTimeSliceType">
        <complexContent>
            <extension base="aixm:AbstractAIXMTimeSliceType">
                <sequence>
                    <group ref="aixm:TaxiHoldingPositionPropertyGroup"/>
                    <element maxOccurs="unbounded" minOccurs="0" name="extension">
                        <complexType>
                            <sequence>
                                <element ref="aixm:AbstractTaxiHoldingPositionExtension"/>
                            </sequence>
                            <attributeGroup ref="gml:OwnershipAttributeGroup"/>
                        </complexType>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <group name="TaxiHoldingPositionPropertyGroup">
        <sequence>
            <element minOccurs="0" name="landingCategory"
                nillable="true" type="aixm:CodeHoldingCategoryType">
                <annotation>
                    <documentation>Type of landing operations for which that holding position is relevant. For example, precision instrument approach category I, precision instrument approach category II, precision instrument approach category III.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="status" nillable="true" type="aixm:CodeStatusOperationsType">
                <annotation>
                    <documentation>An indication of the operational status of the taxi holding position. For example, normal, unserviceable, downgraded, work in progress, etc.</documentation>
                </annotation>
            </element>
            <element maxOccurs="1" minOccurs="0"
                name="associatedGuidanceLine" nillable="true" type="aixm:GuidanceLinePropertyType">
                <annotation>
                    <documentation>(DO272/ED99) The value of a TaxiwayHoldingPosition feature?s attribute idlin shall be identical to the value of the corresponding TaxiwayElement feature?s attribute idlin (Rule 12).</documentation>
                </annotation>
            </element>
            <element maxOccurs="1" minOccurs="0" name="location"
                nillable="true" type="aixm:ElevatedPointPropertyType">
                <annotation>
                    <documentation>The location of the TaxiHoldingPosition.</documentation>
                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0"
                name="protectedRunway" nillable="true" type="aixm:RunwayPropertyType">
                <annotation>
                    <documentation>(DO272/ED99) The value of a TaxiwayHoldingPosition feature?s attribute idp shall be identical to the idrwy object identifier value of the protected real world runway (Rule 6).&#xd;
&#xd;
(DO272/ED99) The value of a TaxiwayHoldingPosition feature?s attribute idp shall be identical to the value of the operationally corresponding RunwayThresholds feature?s attribute idthr (Rule 20).</documentation>
                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0"
                name="annotation" nillable="true" type="aixm:NotePropertyType"/>
        </sequence>
    </group>
    <element abstract="true" name="AbstractTaxiHoldingPositionExtension"
        substitutionGroup="aixm:AbstractExtension" type="aixm:AbstractExtensionType"/>
    <complexType name="TaxiwayPropertyType">
        <attributeGroup ref="gml:OwnershipAttributeGroup"/>
        <attributeGroup ref="gml:AssociationAttributeGroup"/>
    </complexType>
    <element name="Taxiway" substitutionGroup="aixm:AbstractAIXMFeature" type="aixm:TaxiwayType">
        <annotation>
            <documentation>A defined path at an aerodrome/heliport established for the taxiing of aircraft/helicopters and intended to provide a link between one part of the aerodrome and another, including aircraft/helicopter stand taxi-lines, apron taxiways, rapid exit taxiways, air taxiways etc.</documentation>
        </annotation>
    </element>
    <complexType name="TaxiwayType">
        <complexContent>
            <extension base="aixm:AbstractAIXMFeatureType">
                <sequence>
                    <element maxOccurs="unbounded" name="timeSlice" type="aixm:TaxiwayTimeSlicePropertyType"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="TaxiwayTimeSlicePropertyType">
        <sequence>
            <element ref="aixm:TaxiwayTimeSlice"/>
        </sequence>
        <attributeGroup ref="gml:OwnershipAttributeGroup"/>
    </complexType>
    <element name="TaxiwayTimeSlice"
        substitutionGroup="gml:AbstractTimeSlice" type="aixm:TaxiwayTimeSliceType"/>
    <complexType name="TaxiwayTimeSliceType">
        <complexContent>
            <extension base="aixm:AbstractAIXMTimeSliceType">
                <sequence>
                    <group ref="aixm:TaxiwayPropertyGroup"/>
                    <element maxOccurs="unbounded" minOccurs="0" name="extension">
                        <complexType>
                            <sequence>
                                <element ref="aixm:AbstractTaxiwayExtension"/>
                            </sequence>
                            <attributeGroup ref="gml:OwnershipAttributeGroup"/>
                        </complexType>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <group name="TaxiwayPropertyGroup">
        <sequence>
