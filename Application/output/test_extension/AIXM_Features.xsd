                <annotation>
                    <documentation>Prefix used in support of current naming conventions</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="approachType" nillable="true" type="aixm:CodeApproachType">
                <annotation>
                    <documentation>A name describing the type of radio navigational aid/system</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="multipleIdentification"
                nillable="true" type="aixm:CodeUpperAlphaType">
