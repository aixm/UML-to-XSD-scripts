                    <documentation>An indicator of the order of the component in an aggregation.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="theAirspaceVolume"
                nillable="true" type="aixm:AirspaceVolumePropertyType"/>
            <element maxOccurs="unbounded" minOccurs="0"
                name="annotation" nillable="true" type="aixm:NotePropertyType"/>
        </sequence>
    </group>
    <complexType name="AirspaceGeometryComponentPropertyType">
        <complexContent>
            <extension base="aixm:AbstractAIXMPropertyType">
                <sequence>
                    <element ref="aixm:AirspaceGeometryComponent"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <element abstract="true"
        name="AbstractAirspaceGeometryComponentExtension"
        substitutionGroup="aixm:AbstractExtension" type="aixm:AbstractExtensionType"/>
    <element name="AirspaceLayerClass"
        substitutionGroup="aixm:AbstractPropertiesWithSchedule" type="aixm:AirspaceLayerClassType">
