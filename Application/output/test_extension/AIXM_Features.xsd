    <complexType name="NavigationSystemCheckpointPropertyType">
        <attributeGroup ref="gml:OwnershipAttributeGroup"/>
        <attributeGroup ref="gml:AssociationAttributeGroup"/>
    </complexType>
    <element abstract="true" name="AbstractNavigationSystemCheckpoint"
        substitutionGroup="aixm:AbstractAIXMFeature" type="aixm:AbstractNavigationSystemCheckpointType">
        <annotation>
            <documentation>A point established and marked on the surface of an aerodrome allowing the checking of a navigation system (like VOR, GNSS, etc.) or initialisation of an inertial navigation system.&#xd;
</documentation>
        </annotation>
    </element>
    <complexType abstract="true" name="AbstractNavigationSystemCheckpointType">
        <complexContent>
            <extension base="aixm:AbstractAIXMFeatureType"/>
        </complexContent>
    </complexType>
    <group name="NavigationSystemCheckpointPropertyGroup">
        <sequence>
            <element minOccurs="0" name="category" nillable="true" type="aixm:CodeCheckpointCategoryType">
                <annotation>
                    <documentation>Indicates the position of the checkpoint; airborne or ground</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="upperLimit" nillable="true" type="aixm:ValDistanceVerticalType">
                <annotation>
                    <documentation>Indicating the upper limit of the block in which the check should be conducted.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="upperLimitReference"
                nillable="true" type="aixm:CodeVerticalReferenceType">
                <annotation>
                    <documentation>A code indicating the reference for a vertical distance.&#xd;
Two series of values exist:&#xd;
1) real distance: from GND, from the MSL, from the WGS-84 ellipsoid&#xd;
2) pressure distance: QFE, QNH, STD.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="lowerLimit" nillable="true" type="aixm:ValDistanceVerticalType">
                <annotation>
                    <documentation>Indicating the lower limit of the block in which the check should be conducted.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="lowerLimitReference"
                nillable="true" type="aixm:CodeVerticalReferenceType">
                <annotation>
                    <documentation>A code indicating the reference for a vertical distance.&#xd;
Two series of values exist:&#xd;
1) real distance: from GND, from the MSL, from the WGS-84 ellipsoid&#xd;
2) pressure distance: QFE, QNH, STD.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="altitudeInterpretation"
                nillable="true" type="aixm:CodeAltitudeUseType">
                <annotation>
                    <documentation>Indicates how the upper and/or lower altitude values should be interpreted.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="distance" nillable="true" type="aixm:ValDistanceType">
                <annotation>
                    <documentation>The value of the distance from the checkpoint to the navaid.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="angle" nillable="true" type="aixm:ValBearingType">
                <annotation>
                    <documentation>The indication of a bearing (at a given point) by the measurement of the angle between the checkpoint and the navaid equipment (VOR)</documentation>
                </annotation>
            </element>
            <element maxOccurs="1" minOccurs="0" name="position"
                nillable="true" type="aixm:ElevatedPointPropertyType">
                <annotation>
                    <documentation>Identifies the point of he checkpoint.</documentation>
                </annotation>
            </element>
            <element maxOccurs="1" minOccurs="0" name="airportHeliport"
                nillable="true" type="aixm:AirportHeliportPropertyType">
                <annotation>
                    <documentation>Identifies the airport for which the checkpoint is established.</documentation>
                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0"
                name="annotation" nillable="true" type="aixm:NotePropertyType"/>
        </sequence>
    </group>
