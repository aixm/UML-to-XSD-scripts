            <element maxOccurs="1" minOccurs="0" name="centreline"
                nillable="true" type="aixm:CurvePropertyType"/>
            <element maxOccurs="1" minOccurs="0"
                name="horizontalProjection" nillable="true" type="aixm:SurfacePropertyType">
                <annotation>
                    <documentation>The surface defining the horizontal shape of the AirspaceVolume.</documentation>
                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0"
                name="annotation" nillable="true" type="aixm:NotePropertyType"/>
        </sequence>
