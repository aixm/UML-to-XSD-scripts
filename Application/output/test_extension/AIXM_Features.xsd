                <annotation>
                    <documentation>The HoldingPattern assessed by the HoldingAssessment.</documentation>
                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0"
                name="annotation" nillable="true" type="aixm:NotePropertyType"/>
