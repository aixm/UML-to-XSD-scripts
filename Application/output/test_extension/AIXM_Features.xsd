                    <documentation>Identifies the Airport where the Taxiway is situated.</documentation>
                </annotation>
            </element>
            <element maxOccurs="1" minOccurs="0"
                name="surfaceProperties" nillable="true" type="aixm:SurfaceCharacteristicsPropertyType">
                <annotation>
                    <documentation>Identifies the surface characteristics of the taxiway.</documentation>
                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0"
                name="annotation" nillable="true" type="aixm:NotePropertyType"/>
