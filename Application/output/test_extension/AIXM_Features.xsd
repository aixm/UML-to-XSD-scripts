                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0"
                name="availability" nillable="true" type="aixm:ManoeuvringAreaAvailabilityPropertyType">
                <annotation>
                    <documentation>The operational status of the Taxiway.</documentation>
                </annotation>
            </element>
            <element maxOccurs="1" minOccurs="0"
                name="associatedAirportHeliport" nillable="true" type="aixm:AirportHeliportPropertyType">
