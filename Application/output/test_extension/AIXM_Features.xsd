                    <documentation>Identifies the aircraft category for which the Obstacle Assessment Area is evaluated for.</documentation>
                </annotation>
            </element>
            <element maxOccurs="1" minOccurs="0" name="surface"
                nillable="true" type="aixm:SurfacePropertyType">
                <annotation>
                    <documentation>Identifies the extent of the Obstacle Assessment Area.</documentation>
                </annotation>
            </element>
            <element maxOccurs="1" minOccurs="0" name="startingCurve"
                nillable="true" type="aixm:CurvePropertyType">
                <annotation>
                    <documentation>The curve that starts the ObstacleAssessmentArea.</documentation>
                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0"
                name="annotation" nillable="true" type="aixm:NotePropertyType"/>
        </sequence>
