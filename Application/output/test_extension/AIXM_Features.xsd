            <group minOccurs="0" ref="aixm:NavaidEquipmentPropertyGroup"/>
            <element minOccurs="0" name="frequency" nillable="true" type="aixm:ValFrequencyType">
                <annotation>
                    <documentation>The frequency of the localizer.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="magneticBearing"
                nillable="true" type="aixm:ValBearingType">
                <annotation>
                    <documentation>The measured angle between the localizer beam and Magnetic North at the localizer antenna.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="magneticBearingAccuracy"
                nillable="true" type="aixm:ValAngleType">
                <annotation>
                    <documentation>Accuracy of the measured angle between the localizer beam and Magnetic North at the localizer antenna.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="trueBearing" nillable="true" type="aixm:ValBearingType">
                <annotation>
                    <documentation>The measured angle between the localizer beam and True North at the localizer antenna. Note : The True North is the north point at which the meridian lines meet.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="trueBearingAccuracy"
                nillable="true" type="aixm:ValAngleType">
                <annotation>
                    <documentation>Accuracy of the measured angle between the localizer beam and True North at the localizer antenna. </documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="declination" nillable="true" type="aixm:ValMagneticVariationType">
                <annotation>
                    <documentation>The angular difference between True North and the station declination (Magnetic North - as indicated by the station).</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="widthCourse" nillable="true" type="aixm:ValAngleType">
                <annotation>
                    <documentation>The localizer course width, in degrees.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="widthCourseAccuracy"
                nillable="true" type="aixm:ValAngleType">
                <annotation>
                    <documentation>Accuracy of the localizer course width, in degrees.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="backCourseUsable"
                nillable="true" type="aixm:CodeILSBackCourseType">
                <annotation>
                    <documentation>A code indicating the usability of the localizer signal in the back course sector.</documentation>
                </annotation>
            </element>
        </sequence>
