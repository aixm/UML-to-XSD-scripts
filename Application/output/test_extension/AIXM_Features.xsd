            <element maxOccurs="1" minOccurs="0" name="DME"
                nillable="true" type="aixm:DMEPropertyType">
                <annotation>
                    <documentation>Identifies the DME used for a procedure.</documentation>
                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0"
                name="annotation" nillable="true" type="aixm:NotePropertyType"/>
        </sequence>
    </group>
    <element abstract="true" name="AbstractProcedureDMEExtension"
        substitutionGroup="aixm:AbstractExtension" type="aixm:AbstractExtensionType"/>
    <element name="ProcedureTransition" type="aixm:ProcedureTransitionType">
        <annotation>
            <documentation>A group of consecutive segments that are part of a branch  on an approach procedure, SID or STAR.&#xd;
</documentation>
        </annotation>
    </element>
    <complexType name="ProcedureTransitionType">
        <complexContent>
            <extension base="aixm:AbstractAIXMObjectType">
                <sequence>
                    <group ref="aixm:ProcedureTransitionPropertyGroup"/>
                    <element maxOccurs="unbounded" minOccurs="0" name="extension">
                        <complexType>
                            <choice>
                                <element ref="aixm:AbstractProcedureTransitionExtension"/>
                            </choice>
                            <attributeGroup ref="gml:OwnershipAttributeGroup"/>
                        </complexType>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <group name="ProcedureTransitionPropertyGroup">
        <sequence>
            <element minOccurs="0" name="transitionId" nillable="true" type="aixm:CodeDesignatedPointDesignatorType">
                <annotation>
                    <documentation>Used strictly for ARINC coding.  The identifier will be the initial point for each transition.  A transition includes multiple segment legs.  </documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="type" nillable="true" type="aixm:CodeProcedurePhaseType">
                <annotation>
                    <documentation>The type of transition.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="instruction" nillable="true" type="aixm:TextInstructionType">
                <annotation>
                    <documentation>Operational instructions that must be observed when flying the ProcedureTransition part.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="vectorHeading" nillable="true" type="aixm:ValBearingType">
                <annotation>
                    <documentation>A heading provided for use, when a departure is designed to incorporate radar vectors at the termination point,  in the event ATC instructions are not received prior to or at the termination fix.</documentation>
                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0"
                name="transitionLeg" nillable="true" type="aixm:ProcedureTransitionLegType">
                <annotation>
                    <documentation>One segment of a transition.</documentation>
                </annotation>
            </element>
            <element maxOccurs="1" minOccurs="0"
                name="departureRunwayTransition" nillable="true" type="aixm:LandingTakeoffAreaCollectionPropertyType">
                <annotation>
                    <documentation>Business Rule:  This relationship is only used for Departure Runway Transitions.</documentation>
                </annotation>
            </element>
            <element maxOccurs="1" minOccurs="0" name="trajectory"
                nillable="true" type="aixm:CurvePropertyType">
                <annotation>
                    <documentation>Transition is graphically represented by curve</documentation>
                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0"
                name="annotation" nillable="true" type="aixm:NotePropertyType"/>
        </sequence>
    </group>
    <complexType name="ProcedureTransitionPropertyType">
        <complexContent>
            <extension base="aixm:AbstractAIXMPropertyType">
                <sequence>
                    <element ref="aixm:ProcedureTransition"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <element abstract="true" name="AbstractProcedureTransitionExtension"
        substitutionGroup="aixm:AbstractExtension" type="aixm:AbstractExtensionType"/>
    <element name="ProcedureTransitionLeg" type="aixm:ProcedureTransitionLegType">
        <annotation>
            <documentation>Characterises a SegmentLeg which is part of a Procedure Transition&#xd;
</documentation>
        </annotation>
    </element>
    <complexType name="ProcedureTransitionLegType">
        <complexContent>
            <extension base="aixm:AbstractAIXMObjectType">
                <sequence>
                    <group ref="aixm:ProcedureTransitionLegPropertyGroup"/>
                    <element maxOccurs="unbounded" minOccurs="0" name="extension">
                        <complexType>
                            <choice>
                                <element ref="aixm:AbstractProcedureTransitionLegExtension"/>
                            </choice>
                            <attributeGroup ref="gml:OwnershipAttributeGroup"/>
                        </complexType>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <group name="ProcedureTransitionLegPropertyGroup">
        <sequence>
            <element minOccurs="0" name="seqNumberARINC" nillable="true" type="aixm:NoSequenceType">
                <annotation>
                    <documentation>Indicates the order of the segment legs within a transition.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="theSegmentLeg" nillable="true" type="aixm:SegmentLegPropertyType"/>
            <element maxOccurs="unbounded" minOccurs="0"
                name="annotation" nillable="true" type="aixm:NotePropertyType"/>
        </sequence>
    </group>
    <complexType name="ProcedureTransitionLegPropertyType">
        <complexContent>
            <extension base="aixm:AbstractAIXMPropertyType">
                <sequence>
                    <element ref="aixm:ProcedureTransitionLeg"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <element abstract="true"
        name="AbstractProcedureTransitionLegExtension"
        substitutionGroup="aixm:AbstractExtension" type="aixm:AbstractExtensionType"/>
    <complexType name="SegmentLegPropertyType">
        <attributeGroup ref="gml:OwnershipAttributeGroup"/>
        <attributeGroup ref="gml:AssociationAttributeGroup"/>
    </complexType>
    <element abstract="true" name="AbstractSegmentLeg"
        substitutionGroup="aixm:AbstractAIXMFeature" type="aixm:AbstractSegmentLegType">
        <annotation>
            <documentation>A portion of a procedure as defined by two consecutive significant points. &#xd;
</documentation>
        </annotation>
    </element>
    <complexType abstract="true" name="AbstractSegmentLegType">
        <complexContent>
            <extension base="aixm:AbstractAIXMFeatureType"/>
        </complexContent>
    </complexType>
    <group name="SegmentLegPropertyGroup">
        <sequence>
            <element minOccurs="0" name="endConditionDesignator"
                nillable="true" type="aixm:CodeSegmentTerminationType">
                <annotation>
                    <documentation>An indication of the event that allows identifying the end of the leg and the start of the next leg on the procedure.  For example, reaching an altitude or outbound timing.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="legPath" nillable="true" type="aixm:CodeTrajectoryType">
                <annotation>
                    <documentation>A code defining a specific type of flight path such as arc, base turn outbound, base turn inbound, track, proc turn, hold in lieu.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="legTypeARINC" nillable="true" type="aixm:CodeSegmentPathType">
                <annotation>
                    <documentation>A comprehensive description of the 'path and terminator' concept is given in ARINC Specification 424, Attachment 5, Path and Terminator. </documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="course" nillable="true" type="aixm:ValBearingType">
                <annotation>
                    <documentation>The value of the course angle in degrees.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="courseType" nillable="true" type="aixm:CodeCourseType">
                <annotation>
                    <documentation>A code indicating the type of course to be observed. E.g. true track, magnetic track, heading, VOR radial, true bearing, magnetic bearing.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="courseDirection"
                nillable="true" type="aixm:CodeDirectionReferenceType">
                <annotation>
                    <documentation>Indicates flight direction to/from the navaid providing the course guidance.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="turnDirection" nillable="true" type="aixm:CodeDirectionTurnType">
                <annotation>
                    <documentation>A code indicating the direction of the turn.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="speedLimit" nillable="true" type="aixm:ValSpeedType">
                <annotation>
                    <documentation>The value of the speed limit on the procedure leg.  &#xd;
&#xd;
Rules: &#xd;
As a plausibility rule, value should be equivalent to between 0 and 400, when expressed in Km/h.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="speedReference" nillable="true" type="aixm:CodeSpeedReferenceType">
                <annotation>
                    <documentation>A code indicating the reference system for a speed value.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="speedInterpretation"
                nillable="true" type="aixm:CodeAltitudeUseType">
                <annotation>
                    <documentation>A code indicating how the speed attributes of a procedure leg should be interpreted.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="bankAngle" nillable="true" type="aixm:ValAngleType">
                <annotation>
                    <documentation>The recommended bank angle of the aircraft on the turn.  &#xd;
&#xd;
Rules: &#xd;
As a plausibility rule, value should be between 5 and 35 degrees.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="length" nillable="true" type="aixm:ValDistanceType">
                <annotation>
                    <documentation>Length of the segment</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="duration" nillable="true" type="aixm:ValDurationType">
                <annotation>
                    <documentation>Segment duration</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="procedureTurnRequired"
                nillable="true" type="aixm:CodeYesNoType">
                <annotation>
                    <documentation>Indicates if a procedure turn is required at the end of the segmentleg.  Default is No.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="upperLimitAltitude"
                nillable="true" type="aixm:ValDistanceVerticalType">
                <annotation>
                    <documentation>The value of the upper altitude&#xd;
used with codeDescrDistVer:&#xd;
At or below the higher altitude.&#xd;
Between the lower and the upper altitude.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="upperLimitReference"
                nillable="true" type="aixm:CodeVerticalReferenceType">
                <annotation>
                    <documentation>A code indicating the reference for a vertical distance.&#xd;
Two series of values exist:&#xd;
1) real distance: from GND, from the MSL, from the WGS-84 ellipsoid&#xd;
2) pressure distance: QFE, QNH, STD.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="lowerLimitAltitude"
                nillable="true" type="aixm:ValDistanceVerticalType">
                <annotation>
                    <documentation>The lower limit altitude.  &#xd;
&#xd;
used with codeDescrDistVer:&#xd;
At or above the lower altitude.&#xd;
At the lower altitude&#xd;
Between the lower and the upper altitude.&#xd;
Lower altitude is recommended.&#xd;
Expect lower altitude by ATC.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="lowerLimitReference"
                nillable="true" type="aixm:CodeVerticalReferenceType">
                <annotation>
                    <documentation>A code indicating the reference for a vertical distance.&#xd;
Two series of values exist:&#xd;
1) real distance: from GND, from the MSL, from the WGS-84 ellipsoid&#xd;
2) pressure distance: QFE, QNH, STD.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="altitudeInterpretation"
                nillable="true" type="aixm:CodeAltitudeUseType">
                <annotation>
                    <documentation>A code indicating how the altitude attributes of a procedure leg should be interpreted.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="altitudeOverrideATC"
                nillable="true" type="aixm:ValDistanceVerticalType">
                <annotation>
                    <documentation>Alternate altitude when directed by Air Traffic Control</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="altitudeOverrideReference"
                nillable="true" type="aixm:CodeVerticalReferenceType">
                <annotation>
                    <documentation>A code indicating the reference for a vertical distance.&#xd;
Two series of values exist:&#xd;
1) real distance: from GND, from the MSL, from the WGS-84 ellipsoid&#xd;
2) pressure distance: QFE, QNH, STD.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="verticalAngle" nillable="true" type="aixm:ValAngleType">
                <annotation>
                    <documentation>The value of the vertical climb angle of the aircraft on the procedure leg. A negative value means descent.&#xd;
&#xd;
Rules: &#xd;
As a plausibility rule, value should be between -10 and +50 degrees.</documentation>
                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0"
                name="designSurface" nillable="true" type="aixm:ObstacleAssessmentAreaPropertyType">
                <annotation>
                    <documentation>An evaluation area used when designing a procedure segment to insure safety of aircraft </documentation>
                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0"
                name="aircraftCategory" nillable="true" type="aixm:AircraftCharacteristicPropertyType">
                <annotation>
                    <documentation>A segment leg can be established for specific aircraft characteristics.</documentation>
                </annotation>
            </element>
            <element maxOccurs="1" minOccurs="0" name="endPoint"
                nillable="true" type="aixm:TerminalSegmentPointPropertyType">
                <annotation>
                    <documentation>determines the ending point of the segment </documentation>
                </annotation>
            </element>
            <element maxOccurs="1" minOccurs="0" name="arcCentre"
                nillable="true" type="aixm:TerminalSegmentPointPropertyType">
                <annotation>
                    <documentation>An segment known as an Arc can be defined as a radius and distance around a center point.</documentation>
                </annotation>
            </element>
            <element maxOccurs="1" minOccurs="0" name="startPoint"
                nillable="true" type="aixm:TerminalSegmentPointPropertyType">
                <annotation>
                    <documentation>determines the starting point of the segment </documentation>
                </annotation>
            </element>
            <element maxOccurs="1" minOccurs="0" name="angle"
                nillable="true" type="aixm:AngleIndicationPropertyType">
                <annotation>
                    <documentation>Aircraft trajectory must not cross the angle indication.</documentation>
                </annotation>
            </element>
            <element maxOccurs="1" minOccurs="0" name="distance"
                nillable="true" type="aixm:DistanceIndicationPropertyType">
                <annotation>
                    <documentation>Aircraft trajectory must not cross this distance indication.</documentation>
                </annotation>
            </element>
            <element maxOccurs="1" minOccurs="0" name="holding"
                nillable="true" type="aixm:HoldingUseType">
                <annotation>
                    <documentation>Established holding used on a procedure.</documentation>
                </annotation>
            </element>
            <element maxOccurs="1" minOccurs="0" name="trajectory"
                nillable="true" type="aixm:CurvePropertyType">
                <annotation>
                    <documentation>Segment Leg is depicted by the trajectory</documentation>
                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0"
                name="annotation" nillable="true" type="aixm:NotePropertyType"/>
        </sequence>
    </group>
    <element abstract="true" name="AbstractSegmentLegExtension"
        substitutionGroup="aixm:AbstractExtension" type="aixm:AbstractExtensionType"/>
    <element name="ProcedureAvailability"
        substitutionGroup="aixm:AbstractPropertiesWithSchedule" type="aixm:ProcedureAvailabilityType">
        <annotation>
            <documentation>Information about the status of the procedure for flight planning/operations.&#xd;
</documentation>
        </annotation>
    </element>
    <complexType name="ProcedureAvailabilityType">
        <complexContent>
            <extension base="aixm:AbstractPropertiesWithScheduleType">
                <sequence>
                    <group ref="aixm:PropertiesWithSchedulePropertyGroup"/>
                    <group ref="aixm:ProcedureAvailabilityPropertyGroup"/>
                    <element maxOccurs="unbounded" minOccurs="0" name="extension">
                        <complexType>
                            <choice>
                                <element ref="aixm:AbstractPropertiesWithScheduleExtension"/>
                                <element ref="aixm:AbstractProcedureAvailabilityExtension"/>
                            </choice>
                            <attributeGroup ref="gml:OwnershipAttributeGroup"/>
                        </complexType>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <group name="ProcedureAvailabilityPropertyGroup">
        <sequence>
            <element minOccurs="0" name="status" nillable="true" type="aixm:CodeProcedureAvailabilityType">
                <annotation>
                    <documentation>A code indicating the status of the procedure for flight planning/operations.</documentation>
                </annotation>
            </element>
        </sequence>
    </group>
    <complexType name="ProcedureAvailabilityPropertyType">
        <complexContent>
            <extension base="aixm:AbstractAIXMPropertyType">
                <sequence>
                    <element ref="aixm:ProcedureAvailability"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <element abstract="true"
        name="AbstractProcedureAvailabilityExtension"
        substitutionGroup="aixm:AbstractExtension" type="aixm:AbstractExtensionType"/>
    <complexType name="ChangeOverPointPropertyType">
        <attributeGroup ref="gml:OwnershipAttributeGroup"/>
        <attributeGroup ref="gml:AssociationAttributeGroup"/>
    </complexType>
    <element name="ChangeOverPoint"
        substitutionGroup="aixm:AbstractAIXMFeature" type="aixm:ChangeOverPointType">
        <annotation>
            <documentation>The distance from the start of the route portion to the position where change over occurs for VOR defined routes.&#xd;
</documentation>
        </annotation>
    </element>
    <complexType name="ChangeOverPointType">
        <complexContent>
            <extension base="aixm:AbstractAIXMFeatureType">
                <sequence>
                    <element maxOccurs="unbounded" name="timeSlice" type="aixm:ChangeOverPointTimeSlicePropertyType"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="ChangeOverPointTimeSlicePropertyType">
        <sequence>
            <element ref="aixm:ChangeOverPointTimeSlice"/>
        </sequence>
        <attributeGroup ref="gml:OwnershipAttributeGroup"/>
    </complexType>
    <element name="ChangeOverPointTimeSlice"
        substitutionGroup="gml:AbstractTimeSlice" type="aixm:ChangeOverPointTimeSliceType"/>
    <complexType name="ChangeOverPointTimeSliceType">
        <complexContent>
            <extension base="aixm:AbstractAIXMTimeSliceType">
                <sequence>
                    <group ref="aixm:ChangeOverPointPropertyGroup"/>
                    <element maxOccurs="unbounded" minOccurs="0" name="extension">
                        <complexType>
                            <sequence>
                                <element ref="aixm:AbstractChangeOverPointExtension"/>
                            </sequence>
                            <attributeGroup ref="gml:OwnershipAttributeGroup"/>
                        </complexType>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <group name="ChangeOverPointPropertyGroup">
        <sequence>
            <element minOccurs="0" name="distance" nillable="true" type="aixm:ValDistanceType">
                <annotation>
                    <documentation>The distance from the start of the route portion to position of the change over point.</documentation>
                </annotation>
            </element>
            <element maxOccurs="1" minOccurs="0"
                name="applicableRoutePortion" nillable="true" type="aixm:RoutePortionPropertyType">
                <annotation>
                    <documentation>The Route Portion that the change over point applies to.</documentation>
                </annotation>
            </element>
            <choice>
                <annotation>
                    <documentation>A link class that allows selecting between a navaid system, a runway point, an airport reference point, an aiming point or a fix designated point.&#xd;
SignificantPoint accounts for a specified geographical location used to define an ATS route, the flight path of an aircraft or for other navigation/ATS purposes.&#xd;
</documentation>
                </annotation>
                <element maxOccurs="1" minOccurs="0"
                    name="location_fixDesignatedPoint" nillable="true" type="aixm:DesignatedPointPropertyType">
                    <!--No annotation found for this element.-->
                </element>
                <element maxOccurs="1" minOccurs="0"
                    name="location_navaidSystem" nillable="true" type="aixm:NavaidPropertyType">
                    <!--No annotation found for this element.-->
                </element>
                <element maxOccurs="1" minOccurs="0"
                    name="location_position" nillable="true" type="aixm:PointPropertyType">
                    <!--No annotation found for this element.-->
                </element>
                <element maxOccurs="1" minOccurs="0"
                    name="location_runwayPoint" nillable="true" type="aixm:RunwayCentrelinePointPropertyType">
                    <!--No annotation found for this element.-->
                </element>
                <element maxOccurs="1" minOccurs="0"
                    name="location_aimingPoint" nillable="true" type="aixm:TouchDownLiftOffPropertyType">
                    <!--No annotation found for this element.-->
                </element>
                <element maxOccurs="1" minOccurs="0"
                    name="location_airportReferencePoint"
                    nillable="true" type="aixm:AirportHeliportPropertyType">
                    <!--No annotation found for this element.-->
                </element>
            </choice>
            <element maxOccurs="unbounded" minOccurs="0"
                name="annotation" nillable="true" type="aixm:NotePropertyType"/>
        </sequence>
    </group>
    <element abstract="true" name="AbstractChangeOverPointExtension"
        substitutionGroup="aixm:AbstractExtension" type="aixm:AbstractExtensionType"/>
    <complexType name="RoutePropertyType">
        <attributeGroup ref="gml:OwnershipAttributeGroup"/>
        <attributeGroup ref="gml:AssociationAttributeGroup"/>
    </complexType>
    <element name="Route" substitutionGroup="aixm:AbstractAIXMFeature" type="aixm:RouteType">
        <annotation>
            <documentation>A specified route designed for channelling the flow of traffic as necessary for the provision of air traffic services, from the end of the take-off and initial climb phase to the commencement of the approach and landing phase.&#xd;
</documentation>
        </annotation>
    </element>
    <complexType name="RouteType">
        <complexContent>
            <extension base="aixm:AbstractAIXMFeatureType">
                <sequence>
                    <element maxOccurs="unbounded" name="timeSlice" type="aixm:RouteTimeSlicePropertyType"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="RouteTimeSlicePropertyType">
        <sequence>
            <element ref="aixm:RouteTimeSlice"/>
        </sequence>
        <attributeGroup ref="gml:OwnershipAttributeGroup"/>
    </complexType>
    <element name="RouteTimeSlice"
        substitutionGroup="gml:AbstractTimeSlice" type="aixm:RouteTimeSliceType"/>
    <complexType name="RouteTimeSliceType">
        <complexContent>
            <extension base="aixm:AbstractAIXMTimeSliceType">
                <sequence>
                    <group ref="aixm:RoutePropertyGroup"/>
                    <element maxOccurs="unbounded" minOccurs="0" name="extension">
                        <complexType>
                            <sequence>
                                <element ref="aixm:AbstractRouteExtension"/>
                            </sequence>
                            <attributeGroup ref="gml:OwnershipAttributeGroup"/>
                        </complexType>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <group name="RoutePropertyGroup">
        <sequence>
            <element minOccurs="0" name="designatorPrefix"
                nillable="true" type="aixm:CodeRouteDesignatorPrefixType">
                <annotation>
                    <documentation>A prefix for the route designator, indicating whether the route is upper ATS, reserved for helicopters or reserved for supersonic flights.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="designatorSecondLetter"
                nillable="true" type="aixm:CodeRouteDesignatorLetterType">
                <annotation>
                    <documentation>The letter of the route designator. Depending on the region, letters may have special significance with regard to the route usage limitations.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="designatorNumber"
                nillable="true" type="aixm:NoNumberType">
                <annotation>
                    <documentation>The number of the route designator.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="multipleIdentifier"
                nillable="true" type="aixm:CodeUpperAlphaType">
                <annotation>
                    <documentation>A suffix for the route designator, indicating alternates for homonymous routes (especially for military training routes).</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="locationDesignator"
                nillable="true" type="aixm:TextDesignatorType">
                <annotation>
                    <documentation>A textual description of the area in which a designated route is situated.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="name" nillable="true" type="aixm:TextNameType">
                <annotation>
                    <documentation>A free text name by which the route is known.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="type" nillable="true" type="aixm:CodeRouteType">
                <annotation>
                    <documentation>Route classification.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="flightRule" nillable="true" type="aixm:CodeFlightRuleType">
                <annotation>
                    <documentation>A code indicating the flight rules to be observed by aircraft.&#xd;
For example, VFR and/or IFR.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="internationalUse"
                nillable="true" type="aixm:CodeRouteOriginType">
                <annotation>
                    <documentation>A code indicating if the route segment is international or domestic.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="militaryUse" nillable="true" type="aixm:CodeMilitaryStatusType">
                <annotation>
                    <documentation>A code indicating whether the route is for exclusive use by military aircraft.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="militaryTrainingType"
                nillable="true" type="aixm:CodeMilitaryTrainingType">
                <annotation>
                    <documentation>A code indicating the type of military training activity for which the route has been designed.</documentation>
                </annotation>
            </element>
            <element maxOccurs="1" minOccurs="0" name="userOrganisation"
                nillable="true" type="aixm:OrganisationAuthorityPropertyType">
                <annotation>
                    <documentation>Indicates the Organisation normally originating activity on a military training route.</documentation>
