<?xml version="1.0" encoding="UTF-8"?>
<schema attributeFormDefault="unqualified"
    elementFormDefault="qualified"
    targetNamespace="http://www.aixm.aero/schema/5.1.1" version="5.1.1"
    xmlns="http://www.w3.org/2001/XMLSchema"
    xmlns:aixm="http://www.aixm.aero/schema/5.1.1"
    xmlns:gml="http://www.opengis.net/gml/3.2"
    xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <import namespace="http://www.opengis.net/gml/3.2" schemaLocation="http://schemas.opengis.net/gml/3.2.1/gml.xsd"/>
    <import namespace="http://www.w3.org/1999/xlink" schemaLocation="http://www.w3.org/1999/xlink.xsd"/>
    <include schemaLocation="./AIXM_DataTypes.xsd"/>
    <include schemaLocation="./AIXM_AbstractGML_ObjectTypes.xsd"/>
    <complexType name="AerialRefuellingPropertyType">
        <attributeGroup ref="gml:OwnershipAttributeGroup"/>
        <attributeGroup ref="gml:AssociationAttributeGroup"/>
    </complexType>
    <element name="AerialRefuelling"
        substitutionGroup="aixm:AbstractAIXMFeature" type="aixm:AerialRefuellingType">
        <annotation>
            <documentation>A procedure used by the military to transfer fuel from one aircraft to another during flight.</documentation>
        </annotation>
    </element>
    <complexType name="AerialRefuellingType">
        <complexContent>
            <extension base="aixm:AbstractAIXMFeatureType">
                <sequence>
                    <element maxOccurs="unbounded" name="timeSlice" type="aixm:AerialRefuellingTimeSlicePropertyType"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="AerialRefuellingTimeSlicePropertyType">
        <sequence>
            <element ref="aixm:AerialRefuellingTimeSlice"/>
        </sequence>
        <attributeGroup ref="gml:OwnershipAttributeGroup"/>
    </complexType>
    <element name="AerialRefuellingTimeSlice"
        substitutionGroup="gml:AbstractTimeSlice" type="aixm:AerialRefuellingTimeSliceType"/>
    <complexType name="AerialRefuellingTimeSliceType">
        <complexContent>
            <extension base="aixm:AbstractAIXMTimeSliceType">
                <sequence>
                    <group ref="aixm:AerialRefuellingPropertyGroup"/>
                    <element maxOccurs="unbounded" minOccurs="0" name="extension">
                        <complexType>
                            <sequence>
                                <element ref="aixm:AbstractAerialRefuellingExtension"/>
                            </sequence>
                            <attributeGroup ref="gml:OwnershipAttributeGroup"/>
                        </complexType>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <group name="AerialRefuellingPropertyGroup">
        <sequence>
            <element minOccurs="0" name="designatorPrefix"
                nillable="true" type="aixm:CodeAerialRefuellingPrefixType">
                <annotation>
                    <documentation>An affix placed before a route designator used to identify an aerial refuelling route.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="designatorNumber"
                nillable="true" type="aixm:NoNumberType">
                <annotation>
                    <documentation>A designator that identifies the aerial refuelling route.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="designatorSuffix"
                nillable="true" type="aixm:TextDesignatorType">
                <annotation>
                    <documentation>An affix placed after a route designator used to qualify an aerial refuelling route.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="designatorDirection"
                nillable="true" type="aixm:CodeCardinalDirectionType">
                <annotation>
                    <documentation>An indicator of the general cardinal direction of the aerial refuelling procedure</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="name" nillable="true" type="aixm:TextNameType">
                <annotation>
                    <documentation>An identifier by which the aerial refuelling route is known.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="type" nillable="true" type="aixm:CodeAerialRefuellingType">
                <annotation>
                    <documentation>The type of aerial refuelling procedure based on its configuration.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="radarBeaconSetting"
                nillable="true" type="aixm:NoNumberType">
                <annotation>
                    <documentation>The setting to be used by the airborne navigation radar beacon during aerial refuel operations.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="xbandRadarSetting"
                nillable="true" type="aixm:NoNumberType">
                <annotation>
                    <documentation>The setting to be used by the Airborne Identification Radar beacon during aerial refuel operations.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="tankerChannel" nillable="true" type="aixm:CodeTACANChannelType">
                <annotation>
                    <documentation>The Tactical Air Navigation System (TACAN) UHF channel assigned to the tanker aircraft to provide omnidirectional course and distance navigational information during air refuelling operations.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="receiverChannel"
                nillable="true" type="aixm:CodeTACANChannelType">
                <annotation>
                    <documentation>The Tactical Air Navigation System (TACAN) UHF channel assigned to an aircraft receiving fuel to provide omnidirectional course and distance navigational information during air refuelling operations.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="helicopterRoute"
                nillable="true" type="aixm:CodeYesNoType">
                <annotation>
                    <documentation>An indication that the aerial refuelling procedure is designed only for helicopter usage.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="specialRefuelling"
                nillable="true" type="aixm:CodeYesNoType">
                <annotation>
                    <documentation>An indication that the aerial refuelling procedure supports special missions/sortie.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="bidirectionalUse"
                nillable="true" type="aixm:CodeYesNoType">
                <annotation>
                    <documentation>An indication that the aerial refuel track supports simultaneous opposite direction aerial refuelling</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="reverseDirectionTurn"
                nillable="true" type="aixm:CodeDirectionTurnType">
                <annotation>
                    <documentation>An indicator of the turn direction in an aerial manoeuvre (referred to as the Direction of Course Reversal Turn) performed by aircraft during aerial refuelling, where it can reverse its direction of course by turning around either right or left. It only applies to bi-directional routes</documentation>
                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0"
                name="protectingAirspace" nillable="true" type="aixm:AirspacePropertyType">
                <annotation>
                    <documentation>Indicates either the airspace that protects or airspace that are crossed by the Aerial Refuelling procedure.</documentation>
                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0"
                name="availability" nillable="true" type="aixm:RouteAvailabilityPropertyType">
                <annotation>
                    <documentation>The operational availability of the AerialRefuelling.</documentation>
                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0"
                name="managingOrganisation" nillable="true" type="aixm:AuthorityForAerialRefuellingType">
                <annotation>
                    <documentation>The OrganisationAuthority that manages the AerialRefuelling.</documentation>
                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0" name="track"
                nillable="true" type="aixm:AerialRefuellingTrackPropertyType">
                <annotation>
                    <documentation>AerialRefuellingLeg may be either an anchor or a track. In either case, the points that make up an anchor or track are defined in the AerialRefuellingPoint. The Anchor class provides additional anchoring data related to only AR anchors. The geometry of a track is represented by the Curve object and geometryof an anchor is represented by Surface object.</documentation>
                </annotation>
            </element>
            <element maxOccurs="1" minOccurs="0" name="oppositeTrack"
                nillable="true" type="aixm:AerialRefuellingPropertyType">
                <annotation>
                    <documentation>The opposite AerialRefuelling.</documentation>
                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0" name="anchor"
                nillable="true" type="aixm:AerialRefuellingAnchorPropertyType">
                <annotation>
                    <documentation>The definition of the AerialRefuelling anchor.</documentation>
                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0"
                name="annotation" nillable="true" type="aixm:NotePropertyType"/>
        </sequence>
    </group>
    <element abstract="true" name="AbstractAerialRefuellingExtension"
        substitutionGroup="aixm:AbstractExtension" type="aixm:AbstractExtensionType"/>
    <element name="AerialRefuellingAnchor" type="aixm:AerialRefuellingAnchorType">
        <annotation>
            <documentation>A prescribed pattern, established by air refuelling points, along which air-to-air refuelling of aircraft is performed.&#xd;
</documentation>
        </annotation>
    </element>
    <complexType name="AerialRefuellingAnchorType">
        <complexContent>
            <extension base="aixm:AbstractAIXMObjectType">
                <sequence>
                    <group ref="aixm:AerialRefuellingAnchorPropertyGroup"/>
                    <element maxOccurs="unbounded" minOccurs="0" name="extension">
                        <complexType>
                            <choice>
                                <element ref="aixm:AbstractAerialRefuellingAnchorExtension"/>
                            </choice>
                            <attributeGroup ref="gml:OwnershipAttributeGroup"/>
                        </complexType>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <group name="AerialRefuellingAnchorPropertyGroup">
        <sequence>
            <element minOccurs="0" name="outboundCourse" nillable="true" type="aixm:ValBearingType">
                <annotation>
                    <documentation>The radial, course,  bearing or magnetic directional course (if navaid is a localizer) from the facility or waypoint on which anchor is based.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="outboundCourseType"
                nillable="true" type="aixm:CodeCourseType">
                <annotation>
                    <documentation>The type of outbound course from the facility or waypoint on which anchor is based.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="inboundCourse" nillable="true" type="aixm:ValBearingType">
                <annotation>
                    <documentation>The course of the inbound leg.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="turnDirection" nillable="true" type="aixm:CodeDirectionTurnType">
                <annotation>
                    <documentation>Direction of the turn</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="speedLimit" nillable="true" type="aixm:ValSpeedType">
                <annotation>
                    <documentation>Restricted speed for containment in a smaller pattern</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="legSeparation" nillable="true" type="aixm:ValDistanceType">
                <annotation>
                    <documentation>Distance between two parallel legs of the Aerial Refuelling racetrack.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="legLength" nillable="true" type="aixm:ValDistanceType">
                <annotation>
                    <documentation>The length of the Aerial Refuelling racetrack.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="refuellingBaseLevel"
                nillable="true" type="aixm:ValDistanceVerticalType">
                <annotation>
                    <documentation>The lowest altitude or flight level at which refuelling operations can be performed.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="refuellingBaseLevelReference"
                nillable="true" type="aixm:CodeVerticalReferenceType">
                <annotation>
                    <documentation>The vertical reference system for the refuellingBaseLevel distance.</documentation>
                </annotation>
            </element>
            <element maxOccurs="1" minOccurs="0" name="extent"
                nillable="true" type="aixm:SurfacePropertyType">
                <annotation>
                    <documentation>The Surface extent of the AerialRefuellingAnchor.</documentation>
                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0"
                name="verticalExtent" nillable="true" type="aixm:AirspaceLayerPropertyType">
                <annotation>
                    <documentation>The vertical extent of the AerialRefuellingAnchor.</documentation>
                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0" name="point"
                nillable="true" type="aixm:AerialRefuellingPointPropertyType">
                <annotation>
                    <documentation>AerialRefuellingPoint on a AerialRefuellingAnchor.</documentation>
                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0"
                name="annotation" nillable="true" type="aixm:NotePropertyType"/>
        </sequence>
    </group>
    <complexType name="AerialRefuellingAnchorPropertyType">
        <complexContent>
            <extension base="aixm:AbstractAIXMPropertyType">
                <sequence>
                    <element ref="aixm:AerialRefuellingAnchor"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <element abstract="true"
        name="AbstractAerialRefuellingAnchorExtension"
        substitutionGroup="aixm:AbstractExtension" type="aixm:AbstractExtensionType"/>
    <element name="AerialRefuellingPoint"
        substitutionGroup="aixm:AbstractSegmentPoint" type="aixm:AerialRefuellingPointType">
        <annotation>
            <documentation>A geographic position or radio navigation fix along an air refuelling anchor pattern or air refuelling track.&#xd;
</documentation>
        </annotation>
    </element>
    <complexType name="AerialRefuellingPointType">
        <complexContent>
            <extension base="aixm:AbstractSegmentPointType">
                <sequence>
                    <group ref="aixm:SegmentPointPropertyGroup"/>
                    <group ref="aixm:AerialRefuellingPointPropertyGroup"/>
                    <element maxOccurs="unbounded" minOccurs="0" name="extension">
                        <complexType>
                            <choice>
                                <element ref="aixm:AbstractSegmentPointExtension"/>
                                <element ref="aixm:AbstractAerialRefuellingPointExtension"/>
                            </choice>
                            <attributeGroup ref="gml:OwnershipAttributeGroup"/>
                        </complexType>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <group name="AerialRefuellingPointPropertyGroup">
        <sequence>
            <element minOccurs="0" name="sequence" nillable="true" type="aixm:NoSequenceType">
                <annotation>
                    <documentation>This defines the order of points that make up an aerial refuelling track or anchor.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="usageType" nillable="true" type="aixm:CodeAerialRefuellingPointType">
                <annotation>
                    <documentation>The function of the aerial refuelling point in relation to the air refuelling anchor pattern or air refuelling track.</documentation>
                </annotation>
            </element>
        </sequence>
    </group>
    <complexType name="AerialRefuellingPointPropertyType">
        <complexContent>
            <extension base="aixm:AbstractAIXMPropertyType">
                <sequence>
                    <element ref="aixm:AerialRefuellingPoint"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <element abstract="true"
        name="AbstractAerialRefuellingPointExtension"
        substitutionGroup="aixm:AbstractExtension" type="aixm:AbstractExtensionType"/>
    <element name="AerialRefuellingTrack" type="aixm:AerialRefuellingTrackType">
        <annotation>
            <documentation>A sequence of points that define the trajectory to be flown during an aerial refuelling operation.&#xd;
</documentation>
        </annotation>
    </element>
    <complexType name="AerialRefuellingTrackType">
        <complexContent>
            <extension base="aixm:AbstractAIXMObjectType">
                <sequence>
                    <group ref="aixm:AerialRefuellingTrackPropertyGroup"/>
                    <element maxOccurs="unbounded" minOccurs="0" name="extension">
                        <complexType>
                            <choice>
                                <element ref="aixm:AbstractAerialRefuellingTrackExtension"/>
                            </choice>
                            <attributeGroup ref="gml:OwnershipAttributeGroup"/>
                        </complexType>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <group name="AerialRefuellingTrackPropertyGroup">
        <sequence>
            <element maxOccurs="unbounded" minOccurs="0"
                name="verticalExtent" nillable="true" type="aixm:AirspaceLayerPropertyType">
                <annotation>
                    <documentation>The vertical extent of the AerialRefuellingTrack.</documentation>
                </annotation>
            </element>
            <element maxOccurs="1" minOccurs="0" name="extent"
                nillable="true" type="aixm:CurvePropertyType">
                <annotation>
                    <documentation>The shape of the AerialRefueliingTrack.</documentation>
                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0" name="point"
                nillable="true" type="aixm:AerialRefuellingPointPropertyType">
                <annotation>
                    <documentation>AerialRefuellingPoint on the AerialRefuellingTrack</documentation>
                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0"
                name="annotation" nillable="true" type="aixm:NotePropertyType"/>
        </sequence>
    </group>
    <complexType name="AerialRefuellingTrackPropertyType">
        <complexContent>
            <extension base="aixm:AbstractAIXMPropertyType">
                <sequence>
                    <element ref="aixm:AerialRefuellingTrack"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <element abstract="true"
        name="AbstractAerialRefuellingTrackExtension"
        substitutionGroup="aixm:AbstractExtension" type="aixm:AbstractExtensionType"/>
    <element name="AuthorityForAerialRefuelling" type="aixm:AuthorityForAerialRefuellingType">
        <annotation>
            <documentation>The type of authority that the Organisation has over the Aerial Refuelling route.</documentation>
        </annotation>
    </element>
    <complexType name="AuthorityForAerialRefuellingType">
        <complexContent>
            <extension base="aixm:AbstractAIXMObjectType">
                <sequence>
                    <group ref="aixm:AuthorityForAerialRefuellingPropertyGroup"/>
                    <element maxOccurs="unbounded" minOccurs="0" name="extension">
                        <complexType>
                            <choice>
                                <element ref="aixm:AbstractAuthorityForAerialRefuellingExtension"/>
                            </choice>
                            <attributeGroup ref="gml:OwnershipAttributeGroup"/>
                        </complexType>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <group name="AuthorityForAerialRefuellingPropertyGroup">
        <sequence>
            <element minOccurs="0" name="type" nillable="true" type="aixm:CodeAuthorityType">
                <annotation>
                    <documentation>A categorisation of the role that one organisation has for an aerial refuelling route.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="theOrganisationAuthority"
                nillable="true" type="aixm:OrganisationAuthorityPropertyType"/>
            <element maxOccurs="unbounded" minOccurs="0"
                name="annotation" nillable="true" type="aixm:NotePropertyType"/>
        </sequence>
    </group>
    <complexType name="AuthorityForAerialRefuellingPropertyType">
        <complexContent>
            <extension base="aixm:AbstractAIXMPropertyType">
                <sequence>
                    <element ref="aixm:AuthorityForAerialRefuelling"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <element abstract="true"
        name="AbstractAuthorityForAerialRefuellingExtension"
        substitutionGroup="aixm:AbstractExtension" type="aixm:AbstractExtensionType"/>
    <complexType name="AirportHeliportPropertyType">
        <attributeGroup ref="gml:OwnershipAttributeGroup"/>
        <attributeGroup ref="gml:AssociationAttributeGroup"/>
    </complexType>
    <element name="AirportHeliport"
        substitutionGroup="aixm:AbstractAIXMFeature" type="aixm:AirportHeliportType">
        <annotation>
            <documentation>A defined area on land or water (including any buildings, installations and equipment) intended to be used either wholly or in part for the arrival, departure and surface movement of aircraft/helicopters.</documentation>
        </annotation>
    </element>
    <complexType name="AirportHeliportType">
        <complexContent>
            <extension base="aixm:AbstractAIXMFeatureType">
                <sequence>
                    <element maxOccurs="unbounded" name="timeSlice" type="aixm:AirportHeliportTimeSlicePropertyType"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="AirportHeliportTimeSlicePropertyType">
        <sequence>
            <element ref="aixm:AirportHeliportTimeSlice"/>
        </sequence>
        <attributeGroup ref="gml:OwnershipAttributeGroup"/>
    </complexType>
    <element name="AirportHeliportTimeSlice"
        substitutionGroup="gml:AbstractTimeSlice" type="aixm:AirportHeliportTimeSliceType"/>
    <complexType name="AirportHeliportTimeSliceType">
        <complexContent>
            <extension base="aixm:AbstractAIXMTimeSliceType">
                <sequence>
                    <group ref="aixm:AirportHeliportPropertyGroup"/>
                    <element maxOccurs="unbounded" minOccurs="0" name="extension">
                        <complexType>
                            <sequence>
                                <element ref="aixm:AbstractAirportHeliportExtension"/>
                            </sequence>
                            <attributeGroup ref="gml:OwnershipAttributeGroup"/>
                        </complexType>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <group name="AirportHeliportPropertyGroup">
        <sequence>
            <element minOccurs="0" name="designator" nillable="true" type="aixm:CodeAirportHeliportDesignatorType">
                <annotation>
                    <documentation>A designator for an airport/heliport.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="name" nillable="true" type="aixm:TextNameType">
                <annotation>
                    <documentation>The primary official name of an airport/heliport as designated by an appropriate authority.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="locationIndicatorICAO"
                nillable="true" type="aixm:CodeICAOType">
                <annotation>
                    <documentation>The International Civil Aviation Organization (ICAO) location indicator of the airport/heliport, as listed in ICAO DOC 7910.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="designatorIATA" nillable="true" type="aixm:CodeIATAType">
                <annotation>
                    <documentation>The identifier that is assigned to a location in accordance with rules (resolution 767) governed by the International Air Transport Association (IATA).</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="type" nillable="true" type="aixm:CodeAirportHeliportType">
                <annotation>
                    <documentation>The type of airport/heliport. For example, aerodrome only, combined aerodrome/heliport or simple landing site.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="certifiedICAO" nillable="true" type="aixm:CodeYesNoType">
                <annotation>
                    <documentation>Indicating that the airport/heliport is certified according to the International Civil Aviation Organization (ICAO) rules.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="privateUse" nillable="true" type="aixm:CodeYesNoType">
                <annotation>
                    <documentation>An airport or heliport not open for the public. Only for the use of the owners.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="controlType" nillable="true" type="aixm:CodeMilitaryOperationsType">
                <annotation>
                    <documentation>The primary organization type in terms of civil or military, which controls the airport/heliport.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="fieldElevation" nillable="true" type="aixm:ValDistanceVerticalType">
                <annotation>
                    <documentation>The vertical distance above Mean Sea Level (MSL) of the highest point of the landing area.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="fieldElevationAccuracy"
                nillable="true" type="aixm:ValDistanceVerticalType">
                <annotation>
                    <documentation>The vertical distance from the stated elevation within which there is a defined confidence of the true position falling.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="verticalDatum" nillable="true" type="aixm:CodeVerticalDatumType">
                <annotation>
                    <documentation>A reference surface with respect to which elevations and/or depths are specified at the airport/heliport.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="magneticVariation"
                nillable="true" type="aixm:ValMagneticVariationType">
                <annotation>
                    <documentation>The angular difference between True North and Magnetic North measured at a given position and date.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="magneticVariationAccuracy"
                nillable="true" type="aixm:ValAngleType">
                <annotation>
                    <documentation>The accuracy of the Magnetic Variation.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="dateMagneticVariation"
                nillable="true" type="aixm:DateYearType">
                <annotation>
                    <documentation>The date on which an associated magnetic variation was measured at the airport/heliport.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="magneticVariationChange"
                nillable="true" type="aixm:ValMagneticVariationChangeType">
                <annotation>
                    <documentation>The annual rate of change of the magnetic variation at a specified airport/heliport.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="referenceTemperature"
                nillable="true" type="aixm:ValTemperatureType">
                <annotation>
                    <documentation>The monthly mean of the daily maximum temperatures for the hottest month of the year at an airport/heliport.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="altimeterCheckLocation"
                nillable="true" type="aixm:CodeYesNoType">
                <annotation>
                    <documentation>The availability of a point or area designated at an airport/heliport where the checking of an altimeter system can be accomplished.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="secondaryPowerSupply"
