            <element minOccurs="0" name="activity" nillable="true" type="aixm:CodeAirspaceActivityType">
                <annotation>
                    <documentation>The primary situation or reason on the ground or in the air, which may have an impact on airspace usage.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="status" nillable="true" type="aixm:CodeStatusAirspaceType">
                <annotation>
                    <documentation>The activation status of the airspace.</documentation>
                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0" name="levels"
                nillable="true" type="aixm:AirspaceLayerPropertyType">
                <annotation>
                    <documentation>The level and time associated with the airspace usage part.</documentation>
                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0" name="aircraft"
                nillable="true" type="aixm:AircraftCharacteristicPropertyType">
                <annotation>
                    <documentation>Indicates the type of Aircraft that are using the Airspace during its activation.</documentation>
                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0" name="user"
                nillable="true" type="aixm:OrganisationAuthorityPropertyType">
                <annotation>
                    <documentation>The Organisation that uses the Airspace layer.</documentation>
                </annotation>
            </element>
        </sequence>
    </group>
    <complexType name="AirspaceActivationPropertyType">
        <complexContent>
            <extension base="aixm:AbstractAIXMPropertyType">
                <sequence>
                    <element ref="aixm:AirspaceActivation"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <element abstract="true" name="AbstractAirspaceActivationExtension"
        substitutionGroup="aixm:AbstractExtension" type="aixm:AbstractExtensionType"/>
    <element name="AirspaceGeometryComponent" type="aixm:AirspaceGeometryComponentType">
        <annotation>
            <documentation>The role of the component in the airspace geometry. If the geometry of an airspace is composed of single volume, then the attributes of this association class may be left empty.&#xd;
</documentation>
        </annotation>
    </element>
    <complexType name="AirspaceGeometryComponentType">
        <complexContent>
            <extension base="aixm:AbstractAIXMObjectType">
                <sequence>
                    <group ref="aixm:AirspaceGeometryComponentPropertyGroup"/>
