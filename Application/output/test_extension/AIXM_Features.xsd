    </complexType>
    <element name="Navaid" substitutionGroup="aixm:AbstractAIXMFeature" type="aixm:NavaidType">
        <annotation>
            <documentation>A service providing guidance information or position data for the efficient and safe operation of aircraft supported by one or more radio navigation aids.&#xd;
</documentation>
        </annotation>
    </element>
    <complexType name="NavaidType">
        <complexContent>
            <extension base="aixm:AbstractAIXMFeatureType">
                <sequence>
                    <element maxOccurs="unbounded" name="timeSlice" type="aixm:NavaidTimeSlicePropertyType"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="NavaidTimeSlicePropertyType">
        <sequence>
            <element ref="aixm:NavaidTimeSlice"/>
        </sequence>
        <attributeGroup ref="gml:OwnershipAttributeGroup"/>
    </complexType>
    <element name="NavaidTimeSlice"
        substitutionGroup="gml:AbstractTimeSlice" type="aixm:NavaidTimeSliceType"/>
