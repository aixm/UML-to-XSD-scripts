        <attributeGroup ref="gml:OwnershipAttributeGroup"/>
        <attributeGroup ref="gml:AssociationAttributeGroup"/>
    </complexType>
    <element abstract="true" name="AbstractNavaidEquipment"
        substitutionGroup="aixm:AbstractAIXMFeature" type="aixm:AbstractNavaidEquipmentType">
        <annotation>
            <documentation>A physical navaid equipment like VOR, DME, localizer, TACAN, etc..&#xd;
</documentation>
        </annotation>
    </element>
    <complexType abstract="true" name="AbstractNavaidEquipmentType">
        <complexContent>
            <extension base="aixm:AbstractAIXMFeatureType"/>
        </complexContent>
    </complexType>
    <group name="NavaidEquipmentPropertyGroup">
        <sequence>
            <element minOccurs="0" name="designator" nillable="true" type="aixm:CodeNavaidDesignatorType">
                <annotation>
                    <documentation>The identifying code given to the navaid.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="name" nillable="true" type="aixm:TextNameType">
                <annotation>
                    <documentation>The long name given to the navaid equipment.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="emissionClass" nillable="true" type="aixm:CodeRadioEmissionType">
                <annotation>
                    <documentation>A code indicating the type of emission, as defined at the 1979 ITU World Administrative Radio Conference.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="mobile" nillable="true" type="aixm:CodeYesNoType">
                <annotation>
                    <documentation>Indicates if the navaid equipment is mobile.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="magneticVariation"
                nillable="true" type="aixm:ValMagneticVariationType">
                <annotation>
                    <documentation>The measured angle between Magnetic North and True North at the VOR and at the time reported in dateMagneticVariation. By convention, the measure is expressed as a positive number if Magnetic North is to the east of True North and negative if Magnetic North is to the west of True North. Therefore, magnetic bearing + magnetic variation = true bearing. The following rule of thumb applies: ""variation east-magnetic least, variation west-magnetic best"".</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="magneticVariationAccuracy"
                nillable="true" type="aixm:ValAngleType">
                <annotation>
                    <documentation>The accuracy of the measured angle between Magnetic North and True North at the VOR and at the time reported in dateMagneticVariation</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="dateMagneticVariation"
                nillable="true" type="aixm:DateYearType">
