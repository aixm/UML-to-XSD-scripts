    <element name="ArrestingGear"
        substitutionGroup="aixm:AbstractAIXMFeature" type="aixm:ArrestingGearType">
        <annotation>
            <documentation>A series of devices, namely engaging or catching devices and energy absorption devices used to stop an aircraft by absorbing its momentum in a routine, emergency landing or aborted take-off.&#xd;
</documentation>
        </annotation>
    </element>
    <complexType name="ArrestingGearType">
        <complexContent>
            <extension base="aixm:AbstractAIXMFeatureType">
