                    <documentation>One character code for multiple approaches of the same approach type; Z-A</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="copterTrack" nillable="true" type="aixm:ValBearingType">
                <annotation>
                    <documentation>The final approach track value for copter procedures, 1 - 360.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="circlingIdentification"
                nillable="true" type="aixm:CodeUpperAlphaType">
                <annotation>
                    <documentation>A suffix letter for circling only procedures, A - Z.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="courseReversalInstruction"
                nillable="true" type="aixm:TextInstructionType">
                <annotation>
                    <documentation>Operational instructions that must be observed for procedure turns and tear drops.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="additionalEquipment"
                nillable="true" type="aixm:CodeApproachEquipmentAdditionalType">
                <annotation>
                    <documentation>the additional equipment needed to fly the procedure not listed in approachType </documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="channelGNSS" nillable="true" type="aixm:ValChannelNumberType">
                <annotation>
                    <documentation>Operating channel of electronic transceiving equipment</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="WAASReliable" nillable="true" type="aixm:CodeYesNoType">
                <annotation>
                    <documentation>Indicates an expected level of service, e.g., LNAV/VNAV or LPV may not be available.  Wide Area Augmentation System (WAAS).</documentation>
                </annotation>
            </element>
            <element maxOccurs="1" minOccurs="0" name="landing"
                nillable="true" type="aixm:LandingTakeoffAreaCollectionPropertyType">
                <annotation>
                    <documentation>Approach procedure designates the landing areas</documentation>
                </annotation>
            </element>
            <element maxOccurs="1" minOccurs="0" name="finalProfile"
                nillable="true" type="aixm:FinalProfilePropertyType">
                <annotation>
                    <documentation>Approach Procedure declares distances, altitudes and timing for safe landing.</documentation>
                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0"
