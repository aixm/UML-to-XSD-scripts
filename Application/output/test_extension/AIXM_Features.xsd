    <element name="InstrumentApproachProcedure"
        substitutionGroup="aixm:AbstractProcedure" type="aixm:InstrumentApproachProcedureType">
        <annotation>
            <documentation>A series of predetermined manoeuvres by reference to flight instruments with specified protection from obstacles from the initial approach fix, or where applicable , from the beginning of a defined arrival route to a point from which a landing can be completed and thereafter, if a landing is not completed, to a position at which holding or en route obstacle clearance criteria apply.&#xd;
</documentation>
        </annotation>
    </element>
    <complexType name="InstrumentApproachProcedureType">
        <complexContent>
            <extension base="aixm:AbstractProcedureType">
                <sequence>
                    <element maxOccurs="unbounded" name="timeSlice" type="aixm:InstrumentApproachProcedureTimeSlicePropertyType"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="InstrumentApproachProcedureTimeSlicePropertyType">
        <sequence>
            <element ref="aixm:InstrumentApproachProcedureTimeSlice"/>
        </sequence>
        <attributeGroup ref="gml:OwnershipAttributeGroup"/>
