        <complexContent>
            <extension base="aixm:AbstractAIXMTimeSliceType">
                <sequence>
                    <group ref="aixm:MarkerBeaconPropertyGroup"/>
                    <element maxOccurs="unbounded" minOccurs="0" name="extension">
                        <complexType>
                            <choice>
                                <element ref="aixm:AbstractMarkerBeaconExtension"/>
                                <element ref="aixm:AbstractNavaidEquipmentExtension"/>
                            </choice>
                            <attributeGroup ref="gml:OwnershipAttributeGroup"/>
                        </complexType>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <group name="MarkerBeaconPropertyGroup">
        <sequence>
            <group minOccurs="0" ref="aixm:NavaidEquipmentPropertyGroup"/>
            <element minOccurs="0" name="class" nillable="true" type="aixm:CodeMarkerBeaconSignalType">
                <annotation>
                    <documentation>A code indicating the class of the radio marker.&#xd;
For example, fan marker, low-powered fan marker, Z marker.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="frequency" nillable="true" type="aixm:ValFrequencyType">
                <annotation>
                    <documentation>The value of the radio emission frequency.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="axisBearing" nillable="true" type="aixm:ValBearingType">
                <annotation>
                    <documentation>The true bearing of the minor axis of the marker beacon.&#xd;
(source: ARINC 424 Specification)</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="auralMorseCode" nillable="true" type="aixm:CodeAuralMorseType">
