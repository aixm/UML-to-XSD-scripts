    <element name="Localizer"
        substitutionGroup="aixm:AbstractNavaidEquipment" type="aixm:LocalizerType">
        <annotation>
            <documentation>A component of an ILS consisting of a VHF transmitter, radiating signals in the direction served by the ILS, to provide a straight line descent path in the vertical plane containing the centre line of the runway.&#xd;
</documentation>
        </annotation>
    </element>
