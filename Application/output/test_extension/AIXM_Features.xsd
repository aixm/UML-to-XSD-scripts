        <sequence>
            <element ref="aixm:MarkerBeaconTimeSlice"/>
        </sequence>
        <attributeGroup ref="gml:OwnershipAttributeGroup"/>
    </complexType>
    <element name="MarkerBeaconTimeSlice"
        substitutionGroup="gml:AbstractTimeSlice" type="aixm:MarkerBeaconTimeSliceType"/>
