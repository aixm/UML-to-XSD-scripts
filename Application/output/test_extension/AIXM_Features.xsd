        <sequence>
            <element minOccurs="0" name="collocationGroup"
                nillable="true" type="aixm:NoSequenceType">
                <annotation>
                    <documentation>Set of radar equipments that are collocated.  All Radar equipment that has the same value for this property are collocated together. </documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="theRadarEquipment"
                nillable="true" type="aixm:RadarEquipmentPropertyType"/>
            <element maxOccurs="unbounded" minOccurs="0"
                name="annotation" nillable="true" type="aixm:NotePropertyType"/>
        </sequence>
    </group>
    <complexType name="RadarComponentPropertyType">
        <complexContent>
            <extension base="aixm:AbstractAIXMPropertyType">
                <sequence>
                    <element ref="aixm:RadarComponent"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <element abstract="true" name="AbstractRadarComponentExtension"
        substitutionGroup="aixm:AbstractExtension" type="aixm:AbstractExtensionType"/>
    <complexType name="RadarEquipmentPropertyType">
        <attributeGroup ref="gml:OwnershipAttributeGroup"/>
        <attributeGroup ref="gml:AssociationAttributeGroup"/>
    </complexType>
    <element abstract="true" name="AbstractRadarEquipment"
        substitutionGroup="aixm:AbstractAIXMFeature" type="aixm:AbstractRadarEquipmentType">
        <annotation>
            <documentation>Physical radar equipment like PAR, ARSR, ASR, SECRA, etc..&#xd;
</documentation>
        </annotation>
    </element>
    <complexType abstract="true" name="AbstractRadarEquipmentType">
        <complexContent>
            <extension base="aixm:AbstractAIXMFeatureType"/>
        </complexContent>
    </complexType>
    <group name="RadarEquipmentPropertyGroup">
        <sequence>
            <element minOccurs="0" name="name" nillable="true" type="aixm:TextNameType">
                <annotation>
                    <documentation>Textual name of the equipment.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="serialNumber" nillable="true" type="aixm:TextDesignatorType">
                <annotation>
                    <documentation>Serial number of the equipment.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="range" nillable="true" type="aixm:ValDistanceType">
                <annotation>
                    <documentation>The applicable range of the equipment to detect aircraft.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="rangeAccuracy" nillable="true" type="aixm:ValDistanceType">
                <annotation>
                    <documentation>Accuracy of the applicable range of the equipment to detect aircraft.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="dualChannel" nillable="true" type="aixm:CodeYesNoType">
                <annotation>
                    <documentation>Indicates whether the component has dual frequency.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="movingTargetIndicator"
                nillable="true" type="aixm:CodeYesNoType">
                <annotation>
                    <documentation>Indicates whether a Moving Target Indicator (MTI) is available for the component.  The MTI function eliminates ground clutter.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="standbyPower" nillable="true" type="aixm:CodeStandbyPowerType">
                <annotation>
                    <documentation>Indicates how the component is powered  if  the primary power fails.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="digital" nillable="true" type="aixm:CodeYesNoType">
                <annotation>
                    <documentation>Indicates if the radar equipment is digital or analogue.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="militaryUseOnly"
                nillable="true" type="aixm:CodeYesNoType">
                <annotation>
                    <documentation>Indicates the equipment is for military use only.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="specialUseOnly" nillable="true" type="aixm:CodeYesNoType">
                <annotation>
                    <documentation>Indicates the equipment is for special use procedures only such as Helipads, etc.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="specialAircraftOnly"
                nillable="true" type="aixm:CodeYesNoType">
                <annotation>
                    <documentation>Indicates the equipment is to be only used for certain type of aircraft that are capable of using it.  In some instances, the slope angle or procedure can only be flown by high performance aircraft.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="magneticVariation"
                nillable="true" type="aixm:ValMagneticVariationType">
                <annotation>
                    <documentation>The angular difference between True North and Magnetic North measured at a given position and date.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="magneticVariationAccuracy"
                nillable="true" type="aixm:ValAngleType">
                <annotation>
                    <documentation>Accuracy of the magnetic variation.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="dateMagneticVariation"
                nillable="true" type="aixm:DateYearType">
                <annotation>
                    <documentation>The date on which the magnetic variation had this value.</documentation>
                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0" name="contact"
                nillable="true" type="aixm:ContactInformationPropertyType">
                <annotation>
                    <documentation>Identifies the contact information of the group or individual associated with the Radar Equipment.  This will be a contact who is outside of the associated Organisation Authority for the Radar System.  It could be the maintenance technician for the radar system.</documentation>
                </annotation>
            </element>
            <element maxOccurs="1" minOccurs="0" name="location"
                nillable="true" type="aixm:ElevatedPointPropertyType">
                <annotation>
                    <documentation>The geographical point at which the radar is located.</documentation>
                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0"
                name="annotation" nillable="true" type="aixm:NotePropertyType"/>
        </sequence>
    </group>
    <element abstract="true" name="AbstractRadarEquipmentExtension"
        substitutionGroup="aixm:AbstractExtension" type="aixm:AbstractExtensionType"/>
    <complexType name="RadarSystemPropertyType">
        <attributeGroup ref="gml:OwnershipAttributeGroup"/>
        <attributeGroup ref="gml:AssociationAttributeGroup"/>
    </complexType>
    <element name="RadarSystem"
        substitutionGroup="aixm:AbstractAIXMFeature" type="aixm:RadarSystemType">
        <annotation>
            <documentation>One or more Radar Equipment providing radar services.&#xd;
</documentation>
        </annotation>
    </element>
    <complexType name="RadarSystemType">
        <complexContent>
            <extension base="aixm:AbstractAIXMFeatureType">
                <sequence>
                    <element maxOccurs="unbounded" name="timeSlice" type="aixm:RadarSystemTimeSlicePropertyType"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="RadarSystemTimeSlicePropertyType">
        <sequence>
            <element ref="aixm:RadarSystemTimeSlice"/>
        </sequence>
        <attributeGroup ref="gml:OwnershipAttributeGroup"/>
    </complexType>
    <element name="RadarSystemTimeSlice"
        substitutionGroup="gml:AbstractTimeSlice" type="aixm:RadarSystemTimeSliceType"/>
    <complexType name="RadarSystemTimeSliceType">
        <complexContent>
            <extension base="aixm:AbstractAIXMTimeSliceType">
                <sequence>
                    <group ref="aixm:RadarSystemPropertyGroup"/>
                    <element maxOccurs="unbounded" minOccurs="0" name="extension">
                        <complexType>
                            <sequence>
                                <element ref="aixm:AbstractRadarSystemExtension"/>
                            </sequence>
                            <attributeGroup ref="gml:OwnershipAttributeGroup"/>
                        </complexType>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <group name="RadarSystemPropertyGroup">
        <sequence>
            <element minOccurs="0" name="type" nillable="true" type="aixm:CodeRadarServiceType">
                <annotation>
                    <documentation>Defines the type of service for the Radar System.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="model" nillable="true" type="aixm:TextNameType">
                <annotation>
                    <documentation>Model of the equipment ( ASR-11, etc)</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="generalTerrainMonitor"
                nillable="true" type="aixm:CodeYesNoType">
                <annotation>
                    <documentation>General Terrain Monitors (GTM) monitor aircraft clearance above the highest terrain and obstacles in a general area</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="broadcastIdentifier"
                nillable="true" type="aixm:TextDesignatorType">
                <annotation>
                    <documentation>The Identifier broadcasted for a Secondary Radar.</documentation>
                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0"
                name="radarEquipment" nillable="true" type="aixm:RadarComponentType">
                <annotation>
                    <documentation>The equipment that make up the component(s) of a Radar System.</documentation>
                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0" name="office"
                nillable="true" type="aixm:OrganisationAuthorityPropertyType">
                <annotation>
                    <documentation>Defines the Offices that manage the Radar Equipment.</documentation>
                </annotation>
            </element>
            <element maxOccurs="1" minOccurs="0" name="location"
                nillable="true" type="aixm:ElevatedPointPropertyType">
                <annotation>
                    <documentation>This is the location used when the radar system acts as a significant point.  This location is usually the location of one of the radar equipments.</documentation>
                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0"
                name="PARRunway" nillable="true" type="aixm:RunwayPropertyType">
                <annotation>
                    <documentation>Indicates what runway(s) the radar services.</documentation>
                </annotation>
            </element>
            <element maxOccurs="1" minOccurs="0" name="airportHeliport"
                nillable="true" type="aixm:AirportHeliportPropertyType">
                <annotation>
                    <documentation>Identifies the Airport Heliport at whicht the system is located or is near.</documentation>
                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0"
                name="annotation" nillable="true" type="aixm:NotePropertyType"/>
        </sequence>
    </group>
    <element abstract="true" name="AbstractRadarSystemExtension"
        substitutionGroup="aixm:AbstractExtension" type="aixm:AbstractExtensionType"/>
    <element name="Reflector" type="aixm:ReflectorType">
        <annotation>
            <documentation>Describes the various reflectors used with Precision Approach Radars.&#xd;
</documentation>
        </annotation>
    </element>
    <complexType name="ReflectorType">
        <complexContent>
            <extension base="aixm:AbstractAIXMObjectType">
                <sequence>
                    <group ref="aixm:ReflectorPropertyGroup"/>
                    <element maxOccurs="unbounded" minOccurs="0" name="extension">
                        <complexType>
                            <choice>
                                <element ref="aixm:AbstractReflectorExtension"/>
                            </choice>
                            <attributeGroup ref="gml:OwnershipAttributeGroup"/>
                        </complexType>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <group name="ReflectorPropertyGroup">
        <sequence>
            <element minOccurs="0" name="type" nillable="true" type="aixm:CodeReflectorType">
                <annotation>
                    <documentation>Indicates if the reflector is for Touchdown or a Runway End.</documentation>
                </annotation>
            </element>
            <element maxOccurs="1" minOccurs="0"
                name="touchdownReflector" nillable="true" type="aixm:ElevatedPointPropertyType">
                <annotation>
                    <documentation>The geographical point at which the Touchdown Reflector is located.</documentation>
                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0"
                name="annotation" nillable="true" type="aixm:NotePropertyType"/>
        </sequence>
    </group>
    <complexType name="ReflectorPropertyType">
        <complexContent>
            <extension base="aixm:AbstractAIXMPropertyType">
                <sequence>
                    <element ref="aixm:Reflector"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <element abstract="true" name="AbstractReflectorExtension"
        substitutionGroup="aixm:AbstractExtension" type="aixm:AbstractExtensionType"/>
    <complexType name="SecondarySurveillanceRadarPropertyType">
        <attributeGroup ref="gml:OwnershipAttributeGroup"/>
        <attributeGroup ref="gml:AssociationAttributeGroup"/>
    </complexType>
    <element name="SecondarySurveillanceRadar"
        substitutionGroup="aixm:AbstractSurveillanceRadar" type="aixm:SecondarySurveillanceRadarType">
        <annotation>
            <documentation>Secondary Surveillance Radar (SSR aka Secondary Radar (SECRA)) is a radar system in which the object to be detected is fitted with the cooperative equipment in the form of a radio receiver/transmitter (transponder).  Radar pulses transmitted from the searching transmitter/receiver (interrogator) site are received in the cooperative equipment and used to trigger a distinctive transmission from the transponder.  This reply transmission, rather than a reflected signal, is then received back at the transmitter/receiver site for processing and display at an air traffic control facility.&#xd;
</documentation>
        </annotation>
    </element>
    <complexType name="SecondarySurveillanceRadarType">
        <complexContent>
            <extension base="aixm:AbstractSurveillanceRadarType">
                <sequence>
                    <element maxOccurs="unbounded" name="timeSlice" type="aixm:SecondarySurveillanceRadarTimeSlicePropertyType"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="SecondarySurveillanceRadarTimeSlicePropertyType">
        <sequence>
            <element ref="aixm:SecondarySurveillanceRadarTimeSlice"/>
        </sequence>
        <attributeGroup ref="gml:OwnershipAttributeGroup"/>
    </complexType>
    <element name="SecondarySurveillanceRadarTimeSlice"
        substitutionGroup="gml:AbstractTimeSlice" type="aixm:SecondarySurveillanceRadarTimeSliceType"/>
    <complexType name="SecondarySurveillanceRadarTimeSliceType">
        <complexContent>
            <extension base="aixm:AbstractAIXMTimeSliceType">
                <sequence>
                    <group ref="aixm:SecondarySurveillanceRadarPropertyGroup"/>
                    <element maxOccurs="unbounded" minOccurs="0" name="extension">
                        <complexType>
                            <choice>
                                <element ref="aixm:AbstractSecondarySurveillanceRadarExtension"/>
                                <element ref="aixm:AbstractSurveillanceRadarExtension"/>
                                <element ref="aixm:AbstractRadarEquipmentExtension"/>
                            </choice>
                            <attributeGroup ref="gml:OwnershipAttributeGroup"/>
                        </complexType>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <group name="SecondarySurveillanceRadarPropertyGroup">
        <sequence>
            <group minOccurs="0" ref="aixm:SurveillanceRadarPropertyGroup"/>
            <element minOccurs="0" name="transponder" nillable="true" type="aixm:CodeTransponderType">
                <annotation>
                    <documentation>Mode S, or mode select, despite also being called a transponder radar system replacement for Air Traffic Control Radar Beacon System (ATCRBS), is actually a data packet protocol which can be used to augment ATCRBS transponder positioning equipment (radar and TCAS). Mode S is designed to fully interface with ATCRBS systems: mode S SSRs can interrogate ATCRBS transponders, and AIS-P transponders will also reply to older ATCRBS and TCAS interrogations.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="autonomous" nillable="true" type="aixm:CodeYesNoType">
                <annotation>
                    <documentation>Indicates the SRR is autonomous or independent.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="monopulse" nillable="true" type="aixm:CodeYesNoType">
                <annotation>
                    <documentation>Monopulse radar is an adaptation of conical scanning radar which sends additional information in the radar signal in order to avoid problems caused by rapid changes in signal strength. The system also makes jamming more difficult. Most radars designed since the 1960s are monopulse systems.</documentation>
                </annotation>
            </element>
        </sequence>
    </group>
    <element abstract="true"
        name="AbstractSecondarySurveillanceRadarExtension"
        substitutionGroup="aixm:AbstractExtension" type="aixm:AbstractExtensionType"/>
    <element name="SurveillanceGroundStation" type="aixm:SurveillanceGroundStationType">
        <annotation>
            <documentation>Characterizes the ground station where the Radar Scope is utilized.&#xd;
</documentation>
        </annotation>
    </element>
    <complexType name="SurveillanceGroundStationType">
        <complexContent>
            <extension base="aixm:AbstractAIXMObjectType">
                <sequence>
                    <group ref="aixm:SurveillanceGroundStationPropertyGroup"/>
                    <element maxOccurs="unbounded" minOccurs="0" name="extension">
                        <complexType>
                            <choice>
                                <element ref="aixm:AbstractSurveillanceGroundStationExtension"/>
                            </choice>
                            <attributeGroup ref="gml:OwnershipAttributeGroup"/>
                        </complexType>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <group name="SurveillanceGroundStationPropertyGroup">
        <sequence>
            <element minOccurs="0" name="videoMap" nillable="true" type="aixm:CodeYesNoType">
                <annotation>
                    <documentation>Indicates whether a Video Map is available for the component.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="theUnit" nillable="true" type="aixm:UnitPropertyType"/>
            <element maxOccurs="unbounded" minOccurs="0"
                name="annotation" nillable="true" type="aixm:NotePropertyType"/>
        </sequence>
    </group>
    <complexType name="SurveillanceGroundStationPropertyType">
        <complexContent>
            <extension base="aixm:AbstractAIXMPropertyType">
                <sequence>
                    <element ref="aixm:SurveillanceGroundStation"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <element abstract="true"
        name="AbstractSurveillanceGroundStationExtension"
        substitutionGroup="aixm:AbstractExtension" type="aixm:AbstractExtensionType"/>
    <complexType name="SurveillanceRadarPropertyType">
        <attributeGroup ref="gml:OwnershipAttributeGroup"/>
        <attributeGroup ref="gml:AssociationAttributeGroup"/>
    </complexType>
    <element abstract="true" name="AbstractSurveillanceRadar"
        substitutionGroup="aixm:AbstractRadarEquipment" type="aixm:AbstractSurveillanceRadarType">
        <annotation>
            <documentation>An abstract class gathering the properties that are common to both Primary and Secondary Surveillance Radar.&#xd;
</documentation>
        </annotation>
    </element>
    <complexType abstract="true" name="AbstractSurveillanceRadarType">
        <complexContent>
            <extension base="aixm:AbstractRadarEquipmentType"/>
        </complexContent>
    </complexType>
    <group name="SurveillanceRadarPropertyGroup">
        <sequence>
            <group minOccurs="0" ref="aixm:RadarEquipmentPropertyGroup"/>
            <element minOccurs="0" name="verticalCoverageAltitude"
                nillable="true" type="aixm:ValDistanceVerticalType">
                <annotation>
                    <documentation>Altitude for which the Radar provides vertical coverage.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="verticalCoverageDistance"
                nillable="true" type="aixm:ValDistanceType">
                <annotation>
                    <documentation>Distance for which the Radar provides vertical coverage.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="verticalCoverageAzimuth"
                nillable="true" type="aixm:ValBearingType">
                <annotation>
                    <documentation>Azimuth for which the Radar provides vertical coverage.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="antennaTiltFixed"
                nillable="true" type="aixm:CodeYesNoType">
                <annotation>
                    <documentation>Indicates whether the antenna tilt is Variable or Fixed.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="tiltAngle" nillable="true" type="aixm:ValAngleType">
                <annotation>
                    <documentation>Angle at which the antenna is tilted.</documentation>
                </annotation>
            </element>
            <element minOccurs="0" name="automatedRadarTerminalSystem"
                nillable="true" type="aixm:TextDesignatorType">
                <annotation>
                    <documentation>The Automated Radar Terminal System (ARTS) is a system in the air traffic control system using radar intelligence to detect and display pertinent data such as flight identification, altitude, and position of aircraft operating in the terminal area.&#xd;
&#xd;
A Radar with this attribute automatically tracks controlled aircraft and presents alpha-numeric information adjacent to their targets. It may even automatically calculate the separation between different aircrafts and provide an alert if that separation is compromised as per the set standards.</documentation>
                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0"
                name="groundStation" nillable="true" type="aixm:SurveillanceGroundStationType">
                <annotation>
                    <documentation>The gorund station where the Radar Scope is utilized.</documentation>
                </annotation>
            </element>
        </sequence>
    </group>
    <element abstract="true" name="AbstractSurveillanceRadarExtension"
        substitutionGroup="aixm:AbstractExtension" type="aixm:AbstractExtensionType"/>
</schema>
